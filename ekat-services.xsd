		<!--Услуга: Предоставление компенсации расходов, связанных с установкой в муниципальных жилых помещениях приборов учета используемых воды и электрической энергии-->

		<xsd:simpleType name="METERING_DEVICE_TYPE">
			<xsd:annotation>
				<xsd:documentation>Установленные приборы учёта</xsd:documentation>
			</xsd:annotation>
			<xsd:restriction base="xsd:byte">
				<xsd:enumeration value="1">
					<xsd:annotation>
						<xsd:documentation>Прибор учёта воды</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="2">
					<xsd:annotation>
						<xsd:documentation>Прибор учёта электричества</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="3">
					<xsd:annotation>
						<xsd:documentation>Прибор учёта воды и электричества</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleType>


		<xsd:complexType name="MeteringDevicesDataType">
			<xsd:annotation>
				<xsd:documentation>Данные о приборах учёта</xsd:documentation>
			</xsd:annotation>
			<xsd:sequence>
				<xsd:element name="meteringDevicesAdress" type="AdressDataType">
					<xsd:annotation>
						<xsd:documentation>Адрес по котрому установлены приборы учёта.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="type" type="METERING_DEVICE_TYPE">
					<xsd:annotation>
						<xsd:documentation>Установленные приборы учёта</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="ElectricityMeteringDevicesInfo" type="MeteringDevicesInfoDataType">
					<xsd:annotation>
						<xsd:documentation>Сведения о приборах учёта электричества</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="WaterMeteringDevicesInfo" type="MeteringDevicesInfoDataType">
					<xsd:annotation>
						<xsd:documentation>Сведения о приборах учёта воды</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>


		<xsd:complexType name="MeteringDevicesInfoDataType">
			<xsd:annotation>
				<xsd:documentation>Сведения о приборах учёта</xsd:documentation>
			</xsd:annotation>
			<xsd:sequence>
				<xsd:element name="amount" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Количество, штук</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="devicesCost" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Стоимость прибора учета и комплектующих к ним, материалов, необходимых для установки приборов учета, руб.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="installationСost" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Стоимость установки, руб.</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="totalCost" type="xsd:string" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>Итоговая сумма, которую необходимо компенсировать</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>


		<!--Услуга: Выдача документов (выписки из домовой книги, карточки учета собственника жилого помещения, справок и иных документов) в сфере жилищно-коммунального хозяйства-->

		<xsd:simpleType name="HOUSE_TYPE">
			<xsd:annotation>
				<xsd:documentation>Тип жилого помещения, в отношении которого запрашиваются сведения</xsd:documentation>
			</xsd:annotation>
			<xsd:restriction base="xsd:byte">
				<xsd:enumeration value="1">
					<xsd:annotation>
						<xsd:documentation>Индивидуальный жилой дом</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="2">
					<xsd:annotation>
						<xsd:documentation>Коммунальная квартира</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="3">
					<xsd:annotation>
						<xsd:documentation>Отдельная квартира в многоквартирном доме</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleType>

		<xsd:simpleType name="NOTE_TYPE">
			<xsd:annotation>
				<xsd:documentation>Наименование запрашиваемого документа</xsd:documentation>
			</xsd:annotation>
			<xsd:restriction base="xsd:byte">
				<xsd:enumeration value="1">
					<xsd:annotation>
						<xsd:documentation>Справка о регистрации граждан по месту жительства и месту пребывания без указания сведений о ранее зарегистрированных в жилом помещении гражданах</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
				<xsd:enumeration value="2">
					<xsd:annotation>
						<xsd:documentation>Справка о регистрации гражданина по месту жительства и месту пребывания</xsd:documentation>
					</xsd:annotation>
				</xsd:enumeration>
			</xsd:restriction>
		</xsd:simpleType>

		<xsd:complexType name="HouseDocumentsRequestDataType">
			<xsd:annotation>
				<xsd:documentation>Выдача документов</xsd:documentation>
			</xsd:annotation>
			<xsd:sequence>
				<xsd:element name="HouseDocumentsRequest_Adress" type="AdressDataType">
					<xsd:annotation>
						<xsd:documentation>Адрес по которому запрашиваются документы</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="HouseType" type="HOUSE_TYPE">
					<xsd:annotation>
						<xsd:documentation>Тип жилого помещения</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="NoteType" type="NOTE_TYPE">
					<xsd:annotation>
						<xsd:documentation>Наименование запрашиваемого документа</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="RegistrationPeriod" type="date">
					<xsd:annotation>
						<xsd:documentation>Период регистрации</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>



		<!--Услуга: Выдача градостроительных планов земельных участков-->

		<xsd:complexType name="LandPlotDataType">
			<xsd:annotation>
				<xsd:documentation>Сведения о земельном участке</xsd:documentation>
			</xsd:annotation>
			<xsd:sequence>
				<xsd:choice>
					<xsd:element name="LandPlotComplexAdress">
						<xsd:element name="LandPlotAdress" type="AdressDataType">
							<xsd:annotation>
								<xsd:documentation>Адрес расположения земельного участка</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LandPlotInaccurateAdress" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Местоположение земельного участка с привязкой к ориентиру</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="CadastralNumber" type="CadastralNumberDataType">
							<xsd:annotation>
								<xsd:documentation>Кадастровый номер земельного участка</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>
					<xsd:element name="LandPlotAdress" type="AdressDataType">
						<xsd:annotation>
							<xsd:documentation>Адрес расположения земельного участка</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:choice>
					</xsd:sequence>
				</xsd:complexType>


				<!--Услуга: Выдача дубликатов разрешений на строительство и ввод в эксплуатацию объектов капитального строительства-->
				<xsd:simpleType name="ISSUED_PERMIT_TYPE">
					<xsd:annotation>
						<xsd:documentation>Запрашиваемый дубликат разрешения</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Строительство</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Ввод объекта в эксплуатацию</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:complexType name="DetailsIssuedPermitDataType">
					<xsd:annotation>
						<xsd:documentation>Реквизиты выданного разрешения на строительство</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element name="IssuedPermitType" type="ISSUED_PERMIT_TYPE">
							<xsd:annotation>
								<xsd:documentation>Запрашиваемый дубликат разрешения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="IssuedPermitDate" type="date">
							<xsd:annotation>
								<xsd:documentation>Дата выдачи</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="IssuedPermitId" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Номер</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>

				<xsd:complexType name="ConstructionObjectDataType">
					<xsd:annotation>
						<xsd:documentation>Сведения об объекте строительства</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element name="ConstructionObjectName" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Наименование объекта</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="AdministrativeDistrict" type="AdressDataType" minOccurs="0" maxOccurs="1">
							<xsd:annotation>
								<xsd:documentation>Административный район города</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ConstructionObjectAdress" type="AdressDataType">
							<xsd:annotation>
								<xsd:documentation>Адрес объекта</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>


				<xsd:complexType name="DuplicatePermissionsRequestDetailsDataType">
					<xsd:annotation>
						<xsd:documentation>Детали запроса на получение дубликатов разрешений</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element name="DetailsIssuedPermitData" type="ConsDetailsIssuedPermitDataTypetructionObjectDataType">
							<xsd:annotation>
								<xsd:documentation>Реквизиты выданного разрешения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ConstructionObjectData" type="ConstructionObjectDataType">
							<xsd:annotation>
								<xsd:documentation>Сведения об объекте строительства</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="OriginalLossReason" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Причина утраты оригинала</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>


				<!--Услуга: Выдача разрешений на ввод в эксплуатацию, внесение изменений в разрешения на ввод в эксплуатацию объектов капитального строительства-->
				<xsd:simpleType name="CONSTR_OPERATION_TYPE">
					<xsd:annotation>
						<xsd:documentation>Тип строительной операции</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Строительство</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Реконструкция</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:complexType name="DetailsOperationPermitDataType">
					<xsd:annotation>
						<xsd:documentation>Реквизиты выданного разрешения</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element name="OperationPermitType" type="CONSTR_OPERATION_TYPE">
							<xsd:annotation>
								<xsd:documentation>Тип строительной операции</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="OperationPermitDate" type="date">
							<xsd:annotation>
								<xsd:documentation>Дата выдачи</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="OperationPermitId" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Номер</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>

				<xsd:complexType name="IssuancePermitsDetailsDataType">
					<xsd:annotation>
						<xsd:documentation>Детали запроса на выдачу разрешения, внесения изменений в разрешения</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element name="DetailsOperationPermitData" type="DetailsOperationPermitDataType">
							<xsd:annotation>
								<xsd:documentation>Реквизиты разрешения на строительство</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ConstructionObjectData" type="ConstructionObjectDataType">
							<xsd:annotation>
								<xsd:documentation>Сведения об объекте строительства</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="StageDescription" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Описание этапа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="AdministrativeDistrict" type="AdressDataType">
							<xsd:annotation>
								<xsd:documentation>Административный район города</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RequiredChanges" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Изменения, которые требуется внести</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ChangesReason" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Причина внесения изменений</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>



				<!--Услуга: Предоставление сведений из информационной системы обеспечения градостроительной деятельности-->
				<xsd:simpleType name="DOCUMENT_TYPE">
					<xsd:annotation>
						<xsd:documentation>Тип требуемого документа</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Сведения из информационной системы обеспечения</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Копию\выписку\часть документа</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:simpleType name="SUBMISSION_FORM_TYPE">
					<xsd:annotation>
						<xsd:documentation>Форма представления сведений</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>В виде бумажного документа</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>В виде электронной копии документа</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>


				<xsd:simpleType name="AREA_REQUEST_TYPE">
					<xsd:annotation>
						<xsd:documentation>Тип территории, о которой запрашиваются сведения</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>О территории муниципального образования</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>О земельном участке с кадастровым номером</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Об объекте капитального строительства</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:simpleType name="AREA_BORDER_TYPE">
					<xsd:annotation>
						<xsd:documentation>Границы внутри которых находится территория, о которой запрашиваются сведения</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>В границе района(ов), жилого района</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>В квартале улиц</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>В границе улиц</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="4">
							<xsd:annotation>
								<xsd:documentation>На пересечении улиц</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:simpleType name="SECTION_SELECT_TYPE">
					<xsd:annotation>
						<xsd:documentation>Выбор раздела для предоставления сведений</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="0">
							<xsd:annotation>
								<xsd:documentation>Раздел не выбран</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Раздел выбран</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:complexType name="DetailsDocumentsRequestDataType">
					<xsd:annotation>
						<xsd:documentation>Детали запроса на выдачу сведений</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element name="DocumentType" type="DOCUMENT_TYPE">
							<xsd:annotation>
								<xsd:documentation>Тип документа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="AreaRequestType" type="AREA_REQUEST_TYPE">
							<xsd:annotation>
								<xsd:documentation>Тип территории, о которой запрашиваются сведения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="AreaBorderType" type="AREA_BORDER_TYPE">
							<xsd:annotation>
								<xsd:documentation>Границы внутри которых находится территория, о которой запрашиваются сведения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:choice>
							<xsd:choice>
								<xsd:element name="DistrictsList" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Список районов</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="StreetsList" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Список улиц</xsd:documentation>
									</xsd:annotation>
								</xsd:element>

							</xsd:choice>
							<xsd:choice>
								<xsd:element name="CadastralNumber" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Кадастровый номер</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="AreaAdress" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Адрес участка</xsd:documentation>
									</xsd:annotation>
								</xsd:element>

							</xsd:choice>
							<xsd:choice>
								<xsd:element name="CapitalConstruction" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Назначение и адрес объекта капитального строительства</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:choice>
						</xsd:choice>

						<xsd:element name="DocumentChoice_1">
							<xsd:annotation>
								<xsd:documentation>Предоставление информации в виде документа содержащего сведения о документе(ах), размещённых в разделе(ах) информационной системы</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="Section_1" type="SECTION_SELECT_TYPE">
								<xsd:annotation>
									<xsd:documentation>Раздел 1</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Section_2" type="SECTION_SELECT_TYPE">
								<xsd:annotation>
									<xsd:documentation>Раздел 2</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Section_3" type="SECTION_SELECT_TYPE">
								<xsd:annotation>
									<xsd:documentation>Раздел 3</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Section_4" type="SECTION_SELECT_TYPE">
								<xsd:annotation>
									<xsd:documentation>Раздел 4</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Section_5" type="SECTION_SELECT_TYPE">
								<xsd:annotation>
									<xsd:documentation>Раздел 5</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:element>

						<xsd:element name="DocumentChoice_2">
							<xsd:annotation>
								<xsd:documentation>Предоставление информации в виде документа  содержащего сведения об изученности природных и техногенных условий на основании результатов инженерных изысканий </xsd:documentation>
							</xsd:annotation>
							<xsd:element name="EngineeringGeodesic" type="SECTION_SELECT_TYPE">
								<xsd:annotation>
									<xsd:documentation>Инженерно-геодезическое</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="EngineeringGeological" type="SECTION_SELECT_TYPE">
								<xsd:annotation>
									<xsd:documentation>Инженерно-геологическое</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="EngineeringEnvironmental" type="SECTION_SELECT_TYPE">
								<xsd:annotation>
									<xsd:documentation>Инженерно-экологическое</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:element>

						<xsd:element name="DocumentChoice_3">
							<xsd:annotation>
								<xsd:documentation>Предоставление информации в виде документа содержащего сведения об изъятии и резервировании земельных участков для государственных или муниципальных нужд (раздел VII)</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="Section_7" type="SECTION_SELECT_TYPE">
								<xsd:annotation>
									<xsd:documentation>Раздел 7</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:element>

						<xsd:element name="DocumentChoice_4">
							<xsd:annotation>
								<xsd:documentation>Предоставление информации в виде документа содержащего сведения о документах, размещенных в деле о застроенном и подлежащем застройке земельном участке (раздел VIII)</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="Section_8" type="SECTION_SELECT_TYPE">
								<xsd:annotation>
									<xsd:documentation>Раздел 8</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:element>

						<xsd:element name="DocumentChoice_5">
							<xsd:annotation>
								<xsd:documentation>Предоставление информации в виде документа содержащего сведения о картах (схемах) раздела IX «Геодезические и картографические материалы»</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="Section_9" type="SECTION_SELECT_TYPE">
								<xsd:annotation>
									<xsd:documentation>Раздел 9</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>


				<!--Услуга: Согласование местоположения границ земельных участков, являющихся смежными по отношению к земельным участкам, находящимся в муниципальной собственности или в государственной собственности до ее разграничения-->

				<xsd:simpleType name="SELECT_SERVICE_TYPE">
					<xsd:annotation>
						<xsd:documentation>Выбор услуги</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Согласование местоположения границ земельных участков в индивидуальном порядке</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Согласование местоположения границ посредством проведения собрания заинтересованных лиц</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:simpleType name="SELECT_INFO_FIELD_TYPE">
					<xsd:annotation>
						<xsd:documentation>Выбор полей для заполнения сведений о хемельном участке</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Адрес (местоположение) земельного участка</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Кадастровый номер земельного участка</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Адрес (местоположение) и кадастровый номер земельного участка</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>


				<xsd:complexType name="CoordinationLocationLandBoundariesDataType">
					<xsd:annotation>
						<xsd:documentation>Детали о запросе на cогласование местоположения границ земельных участков</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element name="SelectService" type="SELECT_SERVICE_TYPE">
							<xsd:annotation>
								<xsd:documentation>Выбор услуги</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="SelectField" type="SELECT_INFO_FIELD_TYPE">
							<xsd:annotation>
								<xsd:documentation>Выбор полей</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:choice>
							<xsd:choice>
								<xsd:element name="LandPlotInfo">
									<xsd:element name="LandPlotAdress" type="AdressDataType">
										<xsd:annotation>
											<xsd:documentation>Адрес расположения земельного участка</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="CadastralNumber" type="CadastralNumberDataType">
										<xsd:annotation>
											<xsd:documentation>Кадастровый номер земельного участка</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:element>
								<xsd:element name="LandPlotAdress" type="AdressDataType">
									<xsd:annotation>
										<xsd:documentation>Адрес расположения земельного участка</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="CadastralNumber" type="CadastralNumberDataType">
									<xsd:annotation>
										<xsd:documentation>Кадастровый номер земельного участка</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:choice>

							<xsd:element name="MeetingHeldData">
								<xsd:annotation>
									<xsd:documentation>Данные о решении посредством собрания заинтересованных лиц</xsd:documentation>
								</xsd:annotation>
								<xsd:element name="СadastralEngineerInfo">
									<xsd:annotation>
										<xsd:documentation>Информация о кадастровом инженере</xsd:documentation>
									</xsd:annotation>
									<xsd:element name="СadastralEngineerxsd:string" type="xsd:string">
										<xsd:annotation>
											<xsd:documentation>Фамилия, имя, отчество</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="СadastralEngineerCertificate" type="xsd:string">
										<xsd:annotation>
											<xsd:documentation>Номер квалификационного аттестата</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:element>
								<xsd:element name="LandPlotInfo">
									<xsd:element name="LandPlotAdress" type="AdressDataType">
										<xsd:annotation>
											<xsd:documentation>Адрес расположения земельного участка</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="CadastralNumber" type="CadastralNumberDataType">
										<xsd:annotation>
											<xsd:documentation>Кадастровый номер земельного участка</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="AdjacentLandPlotsData" type="xsd:string">
										<xsd:annotation>
											<xsd:documentation>Кадастровый номер земельного участка</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:element>
								<xsd:element name="MeetingInfo">
									<xsd:element name="MeetingAdress" type="AdressDataType">
										<xsd:annotation>
											<xsd:documentation>Адрес проведения собрания</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="MeetingDate" type="date">
										<xsd:annotation>
											<xsd:documentation>Дата проведения собрания</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="MeetingTime" type="xsd:string">
										<xsd:annotation>
											<xsd:documentation>Время проведения собрания</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="DraftBoundaryPlanFamiliarization " type="xsd:string">
										<xsd:annotation>
											<xsd:documentation>Адрес для ознакомления с проектом межевого плана </xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:element>
								<xsd:element name="ObjectionAccepting">
									<xsd:element name="ObjectionAccepting_From" type="date">
										<xsd:annotation>
											<xsd:documentation>Дата начала приёма возражений</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ObjectionAccepting_To" type="date">
										<xsd:annotation>
											<xsd:documentation>Дата конца приёма возражений</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ObjectionsSendingAddress" type="AdressDataType">
										<xsd:annotation>
											<xsd:documentation>Адрес направления обоснованных возражений </xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:element>

							</xsd:element>
						</xsd:choice>
					</xsd:sequence>
				</xsd:complexType>

				<!--Услуга: Прием в собственность муниципального образования «город Екатеринбург» имущества, находящегося в частной собственности-->


				<xsd:simpleType name="CONSTRUCTION_OBJECT_TYPE">
					<xsd:annotation>
						<xsd:documentation>Тип объекта</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Нежилое здание</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Сооружение</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Помещение</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="4">
							<xsd:annotation>
								<xsd:documentation>Земельный участок</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="5">
							<xsd:annotation>
								<xsd:documentation>Иной объект недвижимости</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="6">
							<xsd:annotation>
								<xsd:documentation>Объект движимого имущества</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>


				<xsd:complexType name="PrivatePropertyDataType">
					<xsd:annotation>
						<xsd:documentation>Сведения об объекте частной собственности</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element name="ConstructionObjectType" type="CONSTRUCTION_OBJECT_TYPE">
							<xsd:annotation>
								<xsd:documentation>Тип объекта частной собственности</xsd:documentation>
							</xsd:annotation>
						</xsd:element>


						<xsd:choice>
							<xsd:element name="NonResidentialBuildingType">
								<xsd:annotation>
									<xsd:documentation>Нежилое здание</xsd:documentation>
								</xsd:annotation>
								<xsd:element name="ConstructionObjectName" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Наименование</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ConstructionObjectAdress" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Адрес</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ConstructionObjectFloorsNumber" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Этажность</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ConstructionObjectSquare" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Площадь</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:element>
							<xsd:element name="ConstructionType">
								<xsd:annotation>
									<xsd:documentation>Сооружение</xsd:documentation>
								</xsd:annotation>
								<xsd:element name="ConstructionObjectName" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Наименование</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ConstructionObjectAdress" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Адрес</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ConstructionObjectLength" type="xsd:string" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Протяженность</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ConstructionObjectSquare" type="xsd:string" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Площадь</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:element>
							<xsd:element name="RoomType">
								<xsd:annotation>
									<xsd:documentation>Помещение</xsd:documentation>
								</xsd:annotation>
								<xsd:element name="ConstructionObjectName" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Наименование</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ConstructionObjectAdress" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Адрес</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ConstructionObjectFloor" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Этаж</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ConstructionObjectNumber" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Номер</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ConstructionObjectType">
									<xsd:annotation>
										<xsd:documentation>Тип</xsd:documentation>
									</xsd:annotation>
									<xsd:restriction base="xsd:byte">
										<xsd:enumeration value="1">
											<xsd:annotation>
												<xsd:documentation>Жилое</xsd:documentation>
											</xsd:annotation>
										</xsd:enumeration>
										<xsd:enumeration value="2">
											<xsd:annotation>
												<xsd:documentation>Нежилое</xsd:documentation>
											</xsd:annotation>
										</xsd:enumeration>
									</xsd:restriction>
								</xsd:element>
								<xsd:element name="ConstructionObjectSquare" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Площадь</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:element>
							<xsd:element name="LandPlotType">
								<xsd:annotation>
									<xsd:documentation>Земельный участок</xsd:documentation>
								</xsd:annotation>
								<xsd:element name="ConstructionObjectAdress" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Адрес</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ConstructionObjectSquare" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Площадь</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ConstructionObjectCadastralNumber" type="CadastralNumberDataType">
									<xsd:annotation>
										<xsd:documentation>Кадастровый номер</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:element>
							<xsd:element name="OtherEstateObjectType">
								<xsd:annotation>
									<xsd:documentation>Иной объект недвижимости</xsd:documentation>
								</xsd:annotation>
								<xsd:element name="ConstructionObjectName" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Наименование</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ConstructionObjectAdress" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Адрес</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ConstructionObjectLength" type="xsd:string" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Протяженность</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ConstructionObjectSquare" type="xsd:string" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>Площадь</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:element>
							<xsd:element name="MovablePropertyType">
								<xsd:annotation>
									<xsd:documentation>Объект движимого имущества</xsd:documentation>
								</xsd:annotation>
								<xsd:element name="ConstructionObjectName" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Наименование</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ConstructionObjectFeature" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Индивидуализирующие признаки</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:element>
						</xsd:choice>

						<xsd:element name="ConstructionObjectNote" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Примечание</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>


				<!--Услуга: Отчуждение недвижимого имущества, находящегося в муниципальной собственности и арендуемого субъектами малого и среднего предпринимательства-->

				<xsd:simpleType name="REAL_ESTARE_PROPERTY_TYPE">
					<xsd:annotation>
						<xsd:documentation>Тип объекта</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Здание</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Сооружение</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Нежилое помещение</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:simpleType name="PAYMENT_TERMS_TYPE">
					<xsd:annotation>
						<xsd:documentation>Условия оплаты</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Единовременно</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>В расстрочку</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:complexType name="RealEstatePropertyDataType">
					<xsd:annotation>
						<xsd:documentation>Данные документа о недвижемом имуществе</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element name="RealEstatePropertyType" type="REAL_ESTARE_PROPERTY_TYPE">
							<xsd:annotation>
								<xsd:documentation>Тип объекта недвижимого имущества</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<xsd:element name="RealEstatePropertyArea" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Площадь имущества </xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RealEstatePropertyAdress" type="AdressDataType">
							<xsd:annotation>
								<xsd:documentation>Адрес</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RealEstatePropertyRentContractDate" type="date">
							<xsd:annotation>
								<xsd:documentation>Договор аренды от</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RealEstatePropertyRentContractNumber" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Номер</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<xsd:choice>
							<xsd:element name="BuildingType">
								<xsd:annotation>
									<xsd:documentation>Здание</xsd:documentation>
								</xsd:annotation>
								<xsd:element name="LandPlotArea" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Площадь земельного участка</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:element>
							<xsd:element name="RoomType">
								<xsd:annotation>
									<xsd:documentation>Помещение</xsd:documentation>
								</xsd:annotation>
								<xsd:element name="RealEstatePropertyFloor" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Этаж</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="RealEstatePropertyNumber" type="xsd:string">
									<xsd:annotation>
										<xsd:documentation>Номер</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:element>
						</xsd:choice>

						<xsd:element name="PaymentTerms" type="PAYMENT_TERMS_TYPE">
							<xsd:annotation>
								<xsd:documentation>Условия оплаты</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="InstallmentPeriod" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Срок рассрочки</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="PaymentsFrequency" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Порядок внесения платежей</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

					</xsd:sequence>

				</xsd:complexType>

				<!--Услуга: Предоставление информации об объектах недвижимого имущества, находящихся в муниципальной собственности и предназначенных для сдачи в аренду-->
				<xsd:simpleType name="DISTRICT_ADMINISTRATION_TYPE">
					<xsd:annotation>
						<xsd:documentation>Выбор администрации района для подачи заявления</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Администрация Верх-Исетского района города Екатеринбурга</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Администрация Железнодорожного района города Екатеринбурга</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Администрация Кировского района города Екатеринбурга</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="4">
							<xsd:annotation>
								<xsd:documentation>Администрация Ленинского района города Екатеринбурга</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="5">
							<xsd:annotation>
								<xsd:documentation>Администрация Октябрьского района города Екатеринбурга</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="6">
							<xsd:annotation>
								<xsd:documentation>Администрация Орджоникидзевского района города Екатеринбурга</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="7">
							<xsd:annotation>
								<xsd:documentation>Администрация Чкаловского района города Екатеринбурга</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="8">
							<xsd:annotation>
								<xsd:documentation>Глава Екатеринбурга</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="9">
							<xsd:annotation>
								<xsd:documentation>Администрация города Екатеринбурга</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="10">
							<xsd:annotation>
								<xsd:documentation>Екатеринбургская городская Дума</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="11">
							<xsd:annotation>
								<xsd:documentation>Департамент по управлению муниципальным имуществом</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="12">
							<xsd:annotation>
								<xsd:documentation>Иной орган Администрации города Екатеринбурга</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="13">
							<xsd:annotation>
								<xsd:documentation>Неизвестно</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>


				<xsd:complexType name="MunicipalRealEstateRentDataType">
					<xsd:annotation>
						<xsd:documentation>Данные об аренде муниципального недвижимого имущества</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="MunicipalRealEstateAdress" type="AdressDataType">
						<xsd:annotation>
							<xsd:documentation>Местоположение (адрес) объекта муниципального недвижимого предназначенного для сдачи в аренду</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="MunicipalRealEstateArea" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Площадь необходимая для получения в аренду</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="IntendedUse" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Вид деятельности (целевое назначение)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:complexType>


				<!--Услуга: Принятие документов, а также выдача решений о переводе или об отказе в переводе жилого помещения в нежилое или нежилого помещения в жилое помещение-->
				<xsd:simpleType name="SELECT_TRANSFER_TYPE">
					<xsd:annotation>
						<xsd:documentation>Выберите тип запроса</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Принятие документов и выдача решений о переводе (отказе в переводе) жилого (нежилого) помещения в нежилое (жилое) помещение</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Прием работ по переустройству, и (или) перепланировке, и (или) иных работ для обеспечения использования помещения в качестве жилого или нежилого помещения</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:complexType name="RoomTransferIssuingDecisionsDataType">
					<xsd:annotation>
						<xsd:documentation>Детали запроса о выдаче разрешений о переводе жилого\нежилого помещения</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="SelectTransferType" type="SELECT_TRANSFER_TYPE">
						<xsd:annotation>
							<xsd:documentation>Выбор типа запроса</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="RoomLocation" type="AdressDataType">
						<xsd:annotation>
							<xsd:documentation>Местонахождение помещения</xsd:documentation>
						</xsd:annotation>
					</xsd:element>


					<xsd:choice>

						<xsd:element name="RoomTransfer">

							<xsd:element name="RoomRightsType" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Вид права на помещение</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="RoomRequestType" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Тип запроса</xsd:documentation>
								</xsd:annotation>
								<xsd:restriction base="xsd:byte">
									<xsd:enumeration value="1">
										<xsd:annotation>
											<xsd:documentation>Перевод жилого помещения в нежилое</xsd:documentation>
										</xsd:annotation>
									</xsd:enumeration>
									<xsd:enumeration value="2">
										<xsd:annotation>
											<xsd:documentation>Перевод нежилого помещения в жилое</xsd:documentation>
										</xsd:annotation>
									</xsd:enumeration>
								</xsd:restriction>
							</xsd:element>

							<xsd:element name="RoomIntendedUse" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Целевое назначение помещения</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:element>

						<xsd:element name="RoomRedevelopment">

							<xsd:element name="RoomRedevelopmentFooting " type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Основание переустройства и (или) перепланировки</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="RegulatoryLegalActDate" type="date">
								<xsd:annotation>
									<xsd:documentation>Номер нормативно-правового акта</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="RegulatoryLegalActNumber" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Номер нормативно-правового акта</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="RoomRedevelopmentStartDate" type="date">
								<xsd:annotation>
									<xsd:documentation>Дата начала работ</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="RoomRedevelopmentEndDate" type="date">
								<xsd:annotation>
									<xsd:documentation>Дата окончания работ</xsd:documentation>
								</xsd:annotation>
							</xsd:element>


							<xsd:element name="RoomRedevelopmentPerformerName" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Наименование производителя работ</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="RoomRedevelopmentPerformerPostalData" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Почтовые реквизиты производителя работ</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="RoomRedevelopmentPerformerPhone" type="PhoneDataType">
								<xsd:annotation>
									<xsd:documentation>Телефон производителя работ</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="RoomRedevelopmentPerformerFax" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Факс производителя работ</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="RoomRedevelopmentProjectOrganizationName" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Наименование проектной организации</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="RoomRedevelopmentProjectPostalData" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Почтовые реквизиты проектной организации</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="RoomRedevelopmentProjectPhone" type="PhoneDataType">
								<xsd:annotation>
									<xsd:documentation>Телефон проектной организации</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="RoomRedevelopmentProjectFax" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Факс проектной организации</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:element>

					</xsd:choice>

				</xsd:complexType>


				<!--Услуга: Предоставление информации об объектах учета из реестра муниципального имущества муниципального образования «город Екатеринбург»-->

				<xsd:simpleType name="MUNICIPAL_PROPERTY_TYPE">
					<xsd:annotation>
						<xsd:documentation>Тип объекта</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Жилое помещение</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Нежилое здание, помещение</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Земельный участок</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="5">
							<xsd:annotation>
								<xsd:documentation>Иной объект недвижимости</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="6">
							<xsd:annotation>
								<xsd:documentation>Объект движимого имущества</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>


				<xsd:complexType name="MunicipalPropertyRegisterDataType">
					<xsd:annotation>
						<xsd:documentation>Тип объекта</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="MunicipalPropertyType" type="MUNICIPAL_PROPERTY_TYPE">
						<xsd:annotation>
							<xsd:documentation>Тип мунициального объекта</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="ExtractObtainingType">
						<xsd:annotation>
							<xsd:documentation>Способ получения выписки</xsd:documentation>
						</xsd:annotation>
						<xsd:restriction base="xsd:byte">
							<xsd:enumeration value="1">
								<xsd:annotation>
									<xsd:documentation>Получение документа на личном приеме</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
							<xsd:enumeration value="2">
								<xsd:annotation>
									<xsd:documentation>Получение информации в виде текста в личном кабинете</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
							<xsd:enumeration value="3">
								<xsd:annotation>
									<xsd:documentation>Получение документа в виде электронного документа, подписанного ЭП, в личном кабинете</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
						</xsd:restriction>
					</xsd:element>

				</xsd:complexType>

				<!--Услуга: Предоставление документов (технического паспорта здания (строения) или выписки из него, поэтажного плана, плана земельного участка, экспликации к поэтажному плану, справки об инвентаризационной стоимости объекта недвижимости и иных документов)-->

				<xsd:simpleType name="REAL_ESTATE_OBJECT_TYPE">
					<xsd:annotation>
						<xsd:documentation>Тип объекта недвижимоти</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Комната</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Квартира</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Комната(ы) в комунальной квартире</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="5">
							<xsd:annotation>
								<xsd:documentation>Здание</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:simpleType name="CHECKBOX_TYPE">
					<xsd:annotation>
						<xsd:documentation>Тип чекбокса</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="0">
							<xsd:annotation>
								<xsd:documentation>Не выбрано</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>выбрано</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>

				</xsd:simpleType>

				<xsd:simpleType name="LAND_PLOT_RIGHT_DOCUMENT_TYPE">
					<xsd:annotation>
						<xsd:documentation>Тип документа, удостоверяющего или подтверждающего право на земельный участок</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Договор аренды земельного участка</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Акт о предоставлении земельного участка в бессрочное пользование</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Решение органа местного самоуправления о выделении (предоставления) земельного участка</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="4">
							<xsd:annotation>
								<xsd:documentation>Свидетельство о государственной регистрации права на земельный участок</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="5">
							<xsd:annotation>
								<xsd:documentation>Свидетельство на право собственности на земельный участок</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:simpleType name="CAPITAL_CONSTRUCTION_LEGAL_DOCUMENT_TYPE">
					<xsd:annotation>
						<xsd:documentation>Тип Правоустанавливающего документа об объекте капитального строитества</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Договор купли-продажи, мены, дарения</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Свидетельство о праве на наследство</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Справка о выплате паевого взноса</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="4">
							<xsd:annotation>
								<xsd:documentation>Вступившее в законную силу решение суда о признании права собственности или права пользования объектом капитального строительства</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="5">
							<xsd:annotation>
								<xsd:documentation>Договор долевого участия в строительстве и акт приема-передачи к нему</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="5">
							<xsd:annotation>
								<xsd:documentation>Ордер или договор социального найма</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:complexType name="RealEstateObjectDocumentsDataType">

					<xsd:element name="RealEstateObjecType" type="REAL_ESTATE_OBJECT_TYPE">
						<xsd:annotation>
							<xsd:documentation>Тип объекта недвижимости</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="RealEstateObjectDocumentsList">
						<xsd:annotation>
							<xsd:documentation>Список запрашиваемой технической документации</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="RealEstateObjectTechnicalPassport" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>Технический паспорт жилого помещения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RealEstateObjectInventoryValueCertificate" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>Справку об инвентаризационной стоимости </xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RealEstateObjectTechnicalPassportExtract" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>Выписку из технического паспорта для предоставления в нотариальную контору (нотариусу)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RealEstateObjectPlan" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>План объекта (выкопировка из поэтажного плана с экспликацией)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RealEstateObjectDemolitionCertificate" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>Справку о сносе строения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RealEstateObjectTechnicalCondition" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>Справку о техническом состоянии (проценте износа) строения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RealEstateObjectRegistrationCertificate" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>Справку о регистрации объекта по данным БТИ до 02.08.1999 года</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RealEstateObjectFloorPlan" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>План этажа для целей перевода в нежилое</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RealEstateObjectTransformationConclusion" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>Заключение о преобразовании объекта(ов) недвижимости</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RealEstateObjecRentList" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>Перечень для аренды</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RealEstateObjectTotalAreaCertificate" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>Справку об общей площади</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RealEstateObjectTechnicalEconomicIndicatorsCertificate" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>Справку с технико-экономическими показателями (для сдачи объекта в эксплуатацию)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RealEstateObjectCostCertificate" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>Справку о стоимости для целей налогооблажения физических лиц на дату</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RealEstateObjectOther" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>Иное</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

					<xsd:element name="RealEstateObjectDocumentsDetails">

						<xsd:element name="RealEstateObjectInventoryValueCertificateDate" type="date">
