								<xsd:documentation>Дата для справка об инвентаризационнной стоиомсти</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RealEstateObjectOtherText" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Введённый иной запрашиваемый документ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>
					<xsd:element name="RealEstateObjectAdress" type="AdressDataType">
						<xsd:annotation>
							<xsd:documentation>Адрес объекта</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="RealEstateObjectCoordinates">
						<xsd:annotation>
							<xsd:documentation>Адрес объекта</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="Entrance" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Адрес объекта</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Floor" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Адрес объекта</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="IntercomCode" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Адрес объекта</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Phone" type="PhoneDataType">
							<xsd:annotation>
								<xsd:documentation>Адрес объекта</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

					<xsd:element name="RealEstateObjectArea" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Площадь объекта по документам</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="CommonArea" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Общая площадь</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LivingArea" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Жилая площадь</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="RoomsNumber" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Количество комнат</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="WorkCompletionPeriod">
							<xsd:annotation>
								<xsd:documentation>Срок выполнения работ</xsd:documentation>
							</xsd:annotation>
							<xsd:restriction base="xsd:byte">
								<xsd:enumeration value="1">
									<xsd:annotation>
										<xsd:documentation>Плановый согласно Прейскуранту БТИ</xsd:documentation>
									</xsd:annotation>
								</xsd:enumeration>
								<xsd:enumeration value="2">
									<xsd:annotation>
										<xsd:documentation>Иной, при наличии технической возможности ЕМУП БТИ</xsd:documentation>
