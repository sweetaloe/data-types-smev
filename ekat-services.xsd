<?xml version="1.0" encoding="UTF-8"?>


<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="ekat-services/universal/1.0.3"
	xmlns="urn://x-artefacts-smev-gov-ru/supplementary/commons/1.0.1" elementFormDefault="qualified">


	<xsd:element name="request" type="requestType">
		<xsd:annotation>
			<xsd:documentation>Корневой элемент запроса</xsd:documentation>
		</xsd:annotation>
	</xsd:element>

	<xsd:complexType name="requestType">
		<xsd:annotation>
			<xsd:documentation>Root request element</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Service" type="FormStepServiceType">
				<xsd:annotation>
					<xsd:documentation>Служебная информация</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ServiceName" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Наименование услуги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Goal" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Цель обращения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ServiceResult" type="SERVICE_RESULT_TYPE" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Способ получения результата услуги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DistrictAdministration" type="DISTRICT_ADMINISTRATION_TYPE" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Выбор администрации района для подачи заявления</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PaymentReceipt" type="PAYMENT_RECEIPT_TYPE" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Способ предоставления квитанции об оплате</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CreditBlock" type="BankCreditBlockType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Банковские реквизиты</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecipientCategoryInfo" type="RecipientCategoryInfoType">
				<xsd:annotation>
					<xsd:documentation>Категория заявителя - заявитель\представитель</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecipientTypeData" type="RecipientTypeDataType">
				<xsd:annotation>
					<xsd:documentation>Тип заявителя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RecipientPersonalData" type="RecipientPersonalDataType" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Данные о заявителе</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RepresentativePersonalData" type="RepresentativePersonalDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Данные о представителе</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!--Услуга: Предоставление компенсации расходов, связанных с установкой в муниципальных жилых помещениях приборов учета используемых воды и электрической энергии-->
			<xsd:element name="MeteringDevicesData" type="MeteringDevicesDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Данные о приборах учёта</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!--Услуга: Выдача документов (выписки из домовой книги, карточки учета собственника жилого помещения, справок и иных документов) в сфере жилищно-коммунального хозяйства-->
			<xsd:element name="HouseDocumentsRequestData" type="HouseDocumentsRequestDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Данные о жилом помещении для выдачи документов</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!--Услуга: Выдача градостроительных планов земельных участков-->
			<xsd:element name="LandPlotData" type="LandPlotDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Сведения о земельном участке</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Услуга: Выдача дубликатов разрешений на строительство и ввод в эксплуатацию объектов капитального строительства-->
			<xsd:element name="DuplicatePermissionsRequestDetailsData" type="DuplicatePermissionsRequestDetailsDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Детали запроса на получение дубликатов разрешений</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Услуга: Выдача разрешений на строительство (реконструкцию), продление (прекращение) действия разрешений на строительство (реконструкцию), внесение изменений в разрешения на строительство (реконструкцию) объектов капитального строительства-->
			<xsd:element name="IssuancePermitsDetailsData" type="IssuancePermitsDetailsDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Детали запроса на получение дубликатов разрешений</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Услуга: Предоставление сведений из информационной системы обеспечения градостроительной деятельности-->
			<xsd:element name="DetailsDocumentsRequestData" type="DetailsDocumentsRequestDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Детали запроса на получение сведений из информационной системы обеспечения градостроительной деятельности</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Услуга: Согласование местоположения границ земельных участков, являющихся смежными по отношению к земельным участкам, находящимся в муниципальной собственности или в государственной собственности до ее разграничения-->
			<!-- И услуга: Согласование местоположения границ земельных участков, находящихся в муниципальной собственности или в государственной собственности до ее разграничения-->
			<xsd:element name="CoordinationLocationLandBoundariesData" type="CoordinationLocationLandBoundariesDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Детали о запросе на cогласование местоположения границ земельных участков</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Услуга: Прием в собственность муниципального образования «город Екатеринбург» имущества, находящегося в частной собственности-->

			<xsd:element name="PrivatePropertyData" type="PrivatePropertyDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Детали о запросе на прием в собственность имущества, находящегося в частной собственности</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Услуга: Отчуждение недвижимого имущества, находящегося в муниципальной собственности и арендуемого субъектами малого и среднего предпринимательства-->

			<xsd:element name="RealEstatePropertyData" type="RealEstatePropertyDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>>Данные о недвижемом имуществе</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Услуга: Предоставление информации об объектах недвижимого имущества, находящихся в муниципальной собственности и предназначенных для сдачи в аренду-->

			<xsd:element name="MunicipalRealEstateRentData" type="MunicipalRealEstateRentDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>>Данные о запросе на аренду муниципального недвижемом имуществе</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Услуга: Принятие документов, а также выдача решений о переводе или об отказе в переводе жилого помещения в нежилое или нежилого помещения в жилое помещение-->

			<xsd:element name="RoomTransferIssuingDecisionsData" type="RoomTransferIssuingDecisionsDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>>Данные о запросе на перевод или отказе на перевод жилого помещения в нежилое (нежилое в жилое)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>


			<!--Услуга: Предоставление информации об объектах учета из реестра муниципального имущества муниципального образования «город Екатеринбург»-->

			<xsd:element name="MunicipalPropertyRegisterData" type="MunicipalPropertyRegisterDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>>Данные о запросе на предоставление информации об объектах учета из реестра муниципального имущества</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Услуга: Предоставление документов (технического паспорта здания (строения) или выписки из него, поэтажного плана, плана земельного участка, экспликации к поэтажному плану, справки об инвентаризационной стоимости объекта недвижимости и иных документов)-->

			<xsd:element name="RealEstateObjectDocumentsData" type="RealEstateObjectDocumentsDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>>Данные о запросе на предоставление документов об объекте недвижимости</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Услуга: Выдача архивных справок, архивных выписок и архивных копий по документам, хранящимся в Отделе по делам архивов-->

			<xsd:element name="ArchivesDivisionArchivedDocumentInformationData" type="ArchivesDivisionArchivedDocumentInformationDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>>Данные о запросе на выдачу архивных справок, архивных выписок и архивных копий по документам</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Услуга: Предоставление архивных справок, архивных копий, архивных выписок, информационных писем, связанных с реализацией законных прав и свобод граждан-->

			<xsd:element name="ArchivesStorageArchivedDocumentInformationDataType" type="ArchivesStorageArchivedDocumentInformationDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>>Данные о запросе на предоставление архивных справок, архивных копий, архивных выписок, информационных писем, связанных с реализацией законных прав и свобод граждан</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Услуга: Предоставление информации о времени и месте театральных представлений, филармонических и эстрадных концертов и гастрольных мероприятий театров и филармоний, киносеансов, анонсы данных мероприятий-->
			<!--И услуга: Предоставление информации о проведении ярмарок, выставок народного творчества, ремесел-->
			<xsd:element name="EventsInformationRequestData" type="EventsInformationRequestDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>>Данные о запросе о мероприятиях</xsd:documentation>
				</xsd:annotation>
			</xsd:element>


			<!--Услуга: Запись на обзорные, тематические и интерактивные экскурсии-->

			<xsd:element name="ExcursionData" type="ExcursionDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>>Данные о запросе на запись на экскурсию</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Оформление дубликатов договоров приватизации жилых помещений муниципального жилищного фонда-->
			<xsd:element name="PrivatisationAgreementDuplicateData" type="PrivatisationAgreementDuplicateDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>>Данные о запросе на получение дубликатов договора приватизации</xsd:documentation>
				</xsd:annotation>
			</xsd:element>


			<!--Услуга:Оформление отказа от имени муниципального образования "город Екатеринбург" от преимущественного права покупки жилого помещения-->
			<xsd:element name="LivingSpacePreemptiveRightRefusalData" type="LivingSpacePreemptiveRightRefusalDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>>Данные о запросе на оформление отказа от преимущественного права покупки жилого помещения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Услуга: Признание граждан участниками подпрограммы «Выполнение государственных обязательств по обеспечению жильем категорий граждан, установленных федеральным законодательством» федеральной целевой программы «Жилище» на 2015 - 2020 годы-->
			<xsd:element name="HousingProvisionSocialProgramDataType" type="HousingProvisionSocialProgramDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>>Данные о запросе на признание граждан участниками подпрограммы «Выполнение государственных обязательств по обеспечению жильем категорий граждан, установленных федеральным законодательством» федеральной целевой программы «Жилище» на 2015 - 2020 годы</xsd:documentation>
				</xsd:annotation>
			</xsd:element>




			<!--Услуга: Оформление дубликата договора социального найма жилого помещения муниципального жилищного фонда, договора найма жилого помещения муниципального специализированного жилищного фонда или ордера на жилое помещение-->
			<xsd:element name="SocialEmploymentContractDuplicateData" type="SocialEmploymentContractDuplicateDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Инфомрация необходимая для получения дубликата договора социального найма</xsd:documentation>
				</xsd:annotation>
			</xsd:element>



			<!--Услуга:Предоставление жилого помещения муниципального жилищного фонда по договору социального найма, изменение договора социального найма жилого помещения и его расторжение-->

			<xsd:element name="SocialLeaseAgreementAmendmentData" type="SocialLeaseAgreementAmendmentDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Инфомрация необходимая для получения дубликата договора социального найма</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Услуга: Предоставление однократно бесплатно в собственность граждан земельных участков для индивидуального жилищного строительства-->

			<xsd:element name="LandPlotsOnetimeProvisionData" type="LandPlotsOnetimeProvisionDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Инфомрация необходимая для получения однократно бесплатно в собственность земельного участка</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Услуга: Принятие малоимущих граждан на учет в качестве нуждающихся в жилых помещениях, предоставляемых по договорам социального найма-->
			<!--Услуга: Принятие граждан на учет для целей предоставления жилых помещений в государственном жилищном фонде социального использования Свердловской области-->

			<xsd:element name="ResidentialPremisesCitizensRegistrationData" type="ResidentialPremisesCitizensRegistrationDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Инфомрация необходимая для принятия граждан на учёт в качестве нуждающихся в жилых помещениях</xsd:documentation>
				</xsd:annotation>
			</xsd:element>



			<!--Услуга: Включение заявителей в список граждан, имеющих право быть принятыми в члены жилищно-строительного кооператива, создаваемого в целях обеспечения жильем граждан в соответствии с Федеральным законом от 24.07.2008 № 161-ФЗ «О содействии развитию жилищного строительства»-->
			<xsd:element name="HousingConstructionCooperativeAcceptanceData" type="HousingConstructionCooperativeAcceptanceDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Данные для включения заявителей в список граждан имеющих право быть принятыми в члены жилищно-строительного кооператива</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Услуги: Выдача специального разрешения на автомобильные перевозки тяжеловесных и (или) крупногабаритных грузов по маршрутам, проходящим полностью или частично по автомобильным дорогам местного значения в границах муниципального образования «город Екатеринбург»-->

			<xsd:element name="CargoTransportSpecialPermitIssuanceDataType" type="CargoTransportSpecialPermitIssuanceDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Данные для включения специального разрешения на автомобильные перевозки</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!--Услуга: Прием заявлений и организация предоставления гражданам субсидий на оплату жилых помещений и коммунальных услуг-->
			<xsd:element name="ResidentialPremisesPaymentSubsidiesData" type="ResidentialPremisesPaymentSubsidiesDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Данные для субсидий на оплату жилых помещений</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!--Услуга: Выдача разрешения на вступление в брак лицам, достигшим возраста шестнадцати лет-->
			<xsd:element name="MarriageLicenseData" type="MarriageLicenseDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Данные для выдачи разрешения на вступление в брак лицам, достигшим возраста шестнадцати лет</xsd:documentation>
				</xsd:annotation>
			</xsd:element>



			<!--Услуга: Выдача справки о периоде выплаты компенсации расходов на оплату жилого помещения и коммунальных услуг и ее размере-->
			<xsd:element name="CompensationForExpensesPaymentPeriodData" type="CompensationForExpensesPaymentPeriodDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Данные для справки о периоде выплаты компенсации расходов на оплату жилого помещения и коммунальных услуг и ее размере</xsd:documentation>
				</xsd:annotation>
			</xsd:element>


			<!--Услуга: Зачисление в образовательное учреждение-->
			<xsd:element name="EducationalInstitutionChildrenEnrollmentData" type="EducationalInstitutionChildrenEnrollmentDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Данные для зачисления ребёнка в образовательное учреждение</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Услуга: Предоставление путевок для летнего отдыха детей в лагерях, организованных образовательными организациями, осуществляющими организацию отдыха и оздоровления обучающихся в каникулярное время (с круглосуточным или дневным пребыванием), санаториях и санаторно-оздоровительных лагерях круглогодичного действия, в загородных стационарных оздоровительных лагерях, специализированных (профильных) лагерях, детских лагерях различной тематической направленности -->

			<xsd:element name="ChildrenSummerHolidaysTripData" type="ChildrenSummerHolidaysTripDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Данные для предоставления путёвок для летнего отдыха детей</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Предоставление информации об образовательных программах и учебных планах, рабочих программах учебных курсов, предметов, дисциплин (модулей), годовых календарных учебных графиках-->
			<xsd:element name="EducationalInformationProvidingData" type="EducationalInformationProvidingDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Данные для предоставления информации об образовательных программах и учебных планах, рабочих программах учебных курсов, предметов, дисциплин (модулей), годовых календарных учебных графиках</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Услуга: Выдача выписки из похозяйственной книги-->

			<xsd:element name="HouseholdBookExtractData" type="HouseholdBookExtractDataType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Данные для предоставления выписки из похозяйственной книги</xsd:documentation>
				</xsd:annotation>
			</xsd:element>


