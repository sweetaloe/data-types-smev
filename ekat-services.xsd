				<!--Услуга: Выдача архивных справок, архивных выписок и архивных копий по документам, хранящимся в Отделе по делам архивов-->

				<xsd:simpleType name="ARCHIVES_DEVISION_ARCHIVED_DOCUMENT_TYPE">
					<xsd:annotation>
						<xsd:documentation>Вид архивного документа</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Постановление</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Распоряжение</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Решение</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="4">
							<xsd:annotation>
								<xsd:documentation>Приказ</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="5">
							<xsd:annotation>
								<xsd:documentation>Договор</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="5">
							<xsd:annotation>
								<xsd:documentation>Акт</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="6">
							<xsd:annotation>
								<xsd:documentation>Иной документ</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:simpleType name="ARCHIVES_DIVISION_ARCHIVED_DOCUMENT_DETAILS_TYPE">
					<xsd:annotation>
						<xsd:documentation>Сведения о документе</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Известна точная дата запрашиваемого документа </xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Хронологические рамки запрашиваемого документа</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Известен номер запрашиваемого документа </xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>




				<xsd:complexType name="ArchivesDivisionArchivedDocumentInformationDataType">
					<xsd:annotation>
						<xsd:documentation>Сведения о запрашиваемой архивной информации</xsd:documentation>
					</xsd:annotation>
					<xsd:element name="ArchivedDocumentCopyOrExtract" type="CHECKBOX_TYPE">
						<xsd:annotation>
							<xsd:documentation>Необходима копия архивного документа, выписка из архивного документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ArchivedDocumentCertificate" type="CHECKBOX_TYPE">
						<xsd:annotation>
							<xsd:documentation>Необходима архивная справка</xsd:documentation>
						</xsd:annotation>
					</xsd:element>


					<xsd:element name="ArchivedDocumentCopyOrExtractDetails">
						<xsd:annotation>
							<xsd:documentation>Информация о копии архивного документа, выписки из архивного документа</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="ArchivedDocumentUnverifiedCopy" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>Незаверенная копия документа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ArchivedDocumentType" type="ARCHIVES_DEVISION_ARCHIVED_DOCUMENT_TYPE" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Тип запрашиваемой архивной информации</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ArchivedDocumentDetailsType" type="ARCHIVES_DIVISION_" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Сведения о запрашиваемом документе</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:choice>

							<xsd:element name="ExactDocumentDate" type="date">
								<xsd:annotation>
									<xsd:documentation>Точная дата запрашиваемого документа</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="DocumentChronologicalFrame">
								<xsd:annotation>
									<xsd:documentation>Хронологические рамки запрашиваемого документа</xsd:documentation>
								</xsd:annotation>
								<xsd:element name="DateFrom" type="date">
									<xsd:annotation>
										<xsd:documentation>Дата от</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="DateTo" type="date">
									<xsd:annotation>
										<xsd:documentation>Дата до</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:element>
							<xsd:element name="DocumentNumber" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Известен номер запрашиваемого документа </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:choice>

						<xsd:element name="DocumentIssuedAuthority" type="DISTRICT_ADMINISTRATION_TYPE">
							<xsd:annotation>
								<xsd:documentation>Орган, издавший документ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="DistrictAdministrationName" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Наименование иного органа Администрации города Екатеринбурга</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<xsd:element name="ArchivedDocumentContent" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>О чём запрашиваемый документ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ArchivedDocumentRequestGoal" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Кол-во требуемых экземпляров заверенных копий</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ArchivedDocumentAdditionalInfo" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

					</xsd:element>

					<xsd:element name="ArchivedDocumentCertificateDetails" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Информация о необходимой архивной справке</xsd:documentation>
						</xsd:annotation>

						<xsd:element name="CertificateType" type="xsd:byte" value="1">
							<xsd:annotation>
								<xsd:documentation>Архивная справка о стаже работы в Администрации города Екатеринбурга</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="AdministrationStructuralDivisionName" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Наименование структурного подразделения, органа Администрации</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="PositionName" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Наименование занимаемой должности</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="WorkExperience">
							<xsd:annotation>
								<xsd:documentation>Стаж работы</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="WorkExperienceFrom" type="date">
								<xsd:annotation>
									<xsd:documentation>Стаж работы с</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="WorkExperienceTo" type="date">
								<xsd:annotation>
									<xsd:documentation>Стаж работы до</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:element>
						<xsd:element name="AdditionalInformation" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Архивная справка о стаже работы в Администрации города Екатеринбурга</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>
				</xsd:complexType>




				<!--Предоставление архивных справок, архивных копий, архивных выписок, информационных писем, связанных с реализацией законных прав и свобод граждан-->



				<xsd:simpleType name="ARCHIVES_STORAGE_ARCHIVED_DOCUMENT_TYPE">
					<xsd:annotation>
						<xsd:documentation>Сведения о документе</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Необходима архивная информация о стаже работы</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Необходима архивная информация о размере заработной платы</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Необходима архивная информация о награждениях</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="4">
							<xsd:annotation>
								<xsd:documentation>Необходима иная архивная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>



				<xsd:simpleType name="ArchivesStorageArchivedDocumentInformationDataType">
					<xsd:annotation>
						<xsd:documentation>Выдача архивных справок, архивных копий, архивных выписок по документам, хранящимся в Муниципальном казенном учреждении «Екатеринбургский муниципальный центр хранения архивной документации», и информационных писем.</xsd:documentation>
					</xsd:annotation>
					<xsd:element name="RequestInformation" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Архивная справка о стаже работы в Администрации города Екатеринбурга</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PersonData" type="PersonDataType">
						<xsd:annotation>
							<xsd:documentation>Данные лица, о котором запрашивается архивная информация</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="LastNameChanged" type="CHECKBOX_TYPE">
						<xsd:annotation>
							<xsd:documentation>Была изменена фамилия</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="LastNameChangeData" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Данные о смене фамилии</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="PreviousLastName" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Старая фамилия</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LastnameChangeYear" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Год смены фамилии</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

					<xsd:element name="ArchivesStorageArchivedDocumentType" type="ARCHIVES_STORAGE_ARCHIVED_DOCUMENT_TYPE">
						<xsd:annotation>
							<xsd:documentation>Тип запрашиваемой информации</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="WorkplaseDetails" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Информация о месте работы</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="OrganizationName" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Наименование организации</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="StructialDevisionName" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Наименование структурного подразделения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="PositionName" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Наименование занимаемой должности</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="WorkExperience" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Стаж работы</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="WorkExperienceFrom" type="date">
								<xsd:annotation>
									<xsd:documentation>Стаж работы с</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="WorkExperienceTo" type="date">
								<xsd:annotation>
									<xsd:documentation>Стаж работы до</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:element>
						<xsd:element name="SalaryConfirmation" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Стаж работы</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="SalaryConfirmationFrom" type="date">
								<xsd:annotation>
									<xsd:documentation>Стаж работы с</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="SalaryConfirmationeTo" type="date">
								<xsd:annotation>
									<xsd:documentation>Стаж работы до</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:element>
						<xsd:element name="ParentalLeave" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Отпуск по уходу за ребенком</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="AwardTypeDate" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Вид/дата награждения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="AdditionalInformation" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дополнительная информация</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>
					<xsd:element name="OtherInformationRequestContent" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Содержание запроса</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

				</xsd:complexType>

				<!--Услуга: Предоставление информации о времени и месте театральных представлений, филармонических и эстрадных концертов и гастрольных мероприятий театров и филармоний, киносеансов, анонсы данных мероприятий-->

				<xsd:simpleType name="EVENTS_INFORMATION_TYPE">
					<xsd:annotation>
						<xsd:documentation>Тип информации о мероприятии</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Афиша мероприятий на месяц</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Предоставление сведений о мероприятии</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:complexType name = "EventsInformationRequestDataType">
					<xsd:annotation>
						<xsd:documentation>Данные для получения необходимой информации о мероприятиях</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="EventsInformationType" type="EVENTS_INFORMATION_TYPE">
						<xsd:annotation>
							<xsd:documentation>Тип информации</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:choice>
						<xsd:element name="InstitutionName" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Наименование учреждения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="EventTopic" type="xsd:string" maxOccurs="undounded">
							<xsd:annotation>
								<xsd:documentation>Тематика мероприятия</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:choice>

					<xsd:choice>
						<xsd:element name="EventName" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Наименование мероприятия</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="EventDate">
							<xsd:annotation>
								<xsd:documentation>Дата мероприятия</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="Month" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Месяц</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Year" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Год</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:element>
					</xsd:choice>
				</xsd:complexType>


				<!--Услуга: Запись на обзорные, тематические и интерактивные экскурсии-->

				<xsd:simpleType name="EXCURSION_TYPE">
					<xsd:annotation>
						<xsd:documentation>Тип посещения</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Группа</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Лично</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:simpleType name="AGE_GROUP_TYPE">
					<xsd:annotation>
						<xsd:documentation>Тип посещения</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Дошкольники</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Школьники</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Студенты</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="4">
							<xsd:annotation>
								<xsd:documentation>Пенсионеры</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="5">
							<xsd:annotation>
								<xsd:documentation>Взрослые</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>


				<xsd:complexType name = "ExcursionDataType">
					<xsd:annotation>
						<xsd:documentation>Запись на обзорные, тематические и интерактивные экскурсии</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="ExcursionType" type="EXCURSION_TYPE">
						<xsd:annotation>
							<xsd:documentation>Тип посещения</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:choice>
						<xsd:element name="GroupDetails">
							<xsd:element name="NumberOfPeople" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Количество человек </xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="AgeGroupType" type="AGE_GROUP_TYPE">
								<xsd:annotation>
									<xsd:documentation>Возрастная группа</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:element>
					</xsd:choice>



					<xsd:element name="PermanentExhibitionExcursionType">
						<xsd:annotation>
							<xsd:documentation>Экскурсия по постоянной экспозиции</xsd:documentation>
						</xsd:annotation>
						<xsd:restriction base="xsd:byte">
							<xsd:enumeration value="1">
								<xsd:annotation>
									<xsd:documentation>Обзорная</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
							<xsd:enumeration value="2">
								<xsd:annotation>
									<xsd:documentation>Тематическая</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
						</xsd:restriction>
					</xsd:element>


					<xsd:element name="ExcursionObject">
						<xsd:annotation>
							<xsd:documentation>Объекты, на которых представлены экспозиции</xsd:documentation>
						</xsd:annotation>
						<xsd:restriction base="xsd:byte">
							<xsd:enumeration value="1">
								<xsd:annotation>
									<xsd:documentation>МАУК "Екатеринбургский музей изобразительных искусств"</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
							<xsd:enumeration value="2">
								<xsd:annotation>
									<xsd:documentation>МБУК "Объединенный музей писателей Урала"</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
							<xsd:enumeration value="3">
								<xsd:annotation>
									<xsd:documentation>МБУК "Музей истории Екатеринбурга"</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
							<xsd:enumeration value="4">
								<xsd:annotation>
									<xsd:documentation>МБУК «Екатеринбургский музейный центр народного творчества "Гамаюн"(ул. Гоголя 20/5)</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
							<xsd:enumeration value="5">
								<xsd:annotation>
									<xsd:documentation>МБУК «Муниципальный музей памяти воинов-интернационалистов "Шурави"(пр. Космонавтов, 26)</xsd:documentation>
								</xsd:annotation>
							</xsd:enumeration>
						</xsd:restriction>
					</xsd:element>

					<xsd:element name="ExcursionObjectBranch" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Филиал</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="ExcursionVisitDate" type="date">
						<xsd:annotation>
							<xsd:documentation>Филиал</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="ExcursionVisitTime" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Филиал</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

				</xsd:complexType>


				<!--Услуга: Оформление дубликатов договоров приватизации жилых помещений муниципального жилищного фонда-->

				<xsd:complexType name = "PrivatisationAgreementDuplicateDataType">
					<xsd:annotation>
						<xsd:documentation>Сведения для оформления дубликатов договоров приватизации</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="PersonAgreementTissued" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>ФИО</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="LivingSpaseAdress" type="AdressDataType">
						<xsd:annotation>
							<xsd:documentation>Адрес жилого помещения</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PrivatisationAgreementDate" type="date" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Договор передачи квартиры в собственность граждан от</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PrivatisationAgreementDuplicateReason" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Причина, по которой необходим дубликат</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:complexType>

				<!--Услуга: Оформление отказа от имени муниципального образования "город Екатеринбург" от преимущественного права покупки жилого помещения-->


				<xsd:simpleType name="LIVING_SPACE_TYPE">
					<xsd:annotation>
						<xsd:documentation>Заявитель является собственником</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Доли в праве собственности на жилое помещение</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Комнаты в коммунальной квартире</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>

					</xsd:restriction>
				</xsd:simpleType>


				<xsd:complexType name = "LivingSpacePreemptiveRightRefusalDataType">
					<xsd:annotation>
						<xsd:documentation>Сведения для оформления дубликатов договоров приватизации</xsd:documentation>
					</xsd:annotation>
					<xsd:element name="LivingSpaseType" type="LIVING_SPACE_TYPE">
						<xsd:annotation>
							<xsd:documentation>Тип жилого помещения где заявитель является собственником </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="LivingSpaseAdress" type="AdressDataType">
						<xsd:annotation>
							<xsd:documentation>Адрес объекта собственности</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="LivingSpaseNearbyRoomArea" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Площадь соседних комнат</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="LivingSpasePrice" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Стоимость, за которую заявитель намерен продать комнату/долю в праве собственности</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:complexType>


				<!--Услуга: Признание граждан участниками подпрограммы «Выполнение государственных обязательств по обеспечению жильем категорий граждан, установленных федеральным законодательством» федеральной целевой программы «Жилище» на 2015 - 2020 годы-->

				<xsd:simpleType name="TOGGLE_TYPE">
					<xsd:annotation>
						<xsd:documentation>Тип переключателя\радио-кнопки с 2мя вариантами "да" "нет"</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="0">
							<xsd:annotation>
								<xsd:documentation>Да</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Нет</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:simpleType name="CITIZEN_SOCIAL_HOUSING_TYPE">
					<xsd:annotation>
						<xsd:documentation>Категории граждан, имеющих право на получение социальной выплаты за счёт средств федерального бюджета для преобретения жилого помещения</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Граждане, подвергшиеся воздействию радиации вследствие радиационных аварий и катастроф и приравлненные к ним лица</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Граждане, признанные в установленном порядке вынужденными переселенцами и включенные территориальным органом в сводный список вынужденных переселенцев</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Граждане, выехавшие из районов Крайнего Севера и приравненных к ним местностей не ранее 01.01.1992, имеющие право на получение социальных выплат для приобретения жилья за счёт средств федерального бюджета</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:complexType name = "FamilyCompositionDataType">
					<xsd:annotation>
						<xsd:documentation>Сведения о составе семьи</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="SpouseData" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Данные о супруге</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="SpousePersonalData" type="PersonDataType">
							<xsd:annotation>
								<xsd:documentation>Данные о супруге</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="SpouseIdentificationDocument" type="DocumentBlockType">
							<xsd:annotation>
								<xsd:documentation>Паспортные данные супруга(и)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

					<xsd:element name="ChildData" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Данные о ребёнке</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="ChildPersonalData" type="PersonDataType">
							<xsd:annotation>
								<xsd:documentation>Данные о ребёнке</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ChildIdentificationDocument" type="DocumentBlockType">
							<xsd:annotation>
								<xsd:documentation>Паспортные данные\свидетельство о рождении ребёнка</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>


					<xsd:element name="AnotherFamilyMemberData" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Данные о ином члене семьи</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="AnotherFamilyMemberPersonalData" type="PersonDataType">
							<xsd:annotation>
								<xsd:documentation>Данные о члене семьи</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="AnotherFamilyMemberIdentificationDocument" type="DocumentBlockType">
							<xsd:annotation>
								<xsd:documentation>Документ удостоверяющий личность члена семьи</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

				</xsd:complexType>

				<xsd:complexType name = "HousingProvisionSocialProgramDataType">
					<xsd:annotation>
						<xsd:documentation>Сведения для оформления дубликатов договоров приватизации</xsd:documentation>
					</xsd:annotation>
					<xsd:element name="CitizenSocialCategory" type="CITIZEN_SOCIAL_HOUSING_TYPE">
						<xsd:annotation>
							<xsd:documentation>Наименование категории граждан, имеющих право на получение социальной выплаты за счёт средств федерального бюджета для приобретения жилого помещения </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="CategoryRegistrationDate" type="date">
						<xsd:annotation>
							<xsd:documentation>Дата подачи заявления на постановку на учёт в качестве нуждающихся в улучшении жилищных условий в целях получения жилищной субсидии</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="CategoryRegistrationPlace" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Место постановки на учёт</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="RegistrationFileNumber" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Номер учётного дела</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="CitizenOwnsResidentialPremises" type="TOGGLE_TYPE">
						<xsd:annotation>
							<xsd:documentation>Имеют ли в настоящее время заявитель и члены его семьи жилые помещения для постоянного проживания на территории Российской Федерации</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="FamilyCompositionData" type="FamilyCompositionDataType">
						<xsd:annotation>
							<xsd:documentation>Состав семьи заявителя</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="PersonalVisitDocumentList">
						<xsd:annotation>
							<xsd:documentation>Список документов предоставляемых лично</xsd:documentation>
						</xsd:annotation>

						<xsd:element name="PersonalVisitDocument" type="xsd:string" minOccurs="3" maxOccurs="14">
							<xsd:annotation>
								<xsd:documentation>Наименование документа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

				</xsd:complexType>


				<!--Услуга: Оформление дубликата договора социального найма жилого помещения муниципального жилищного фонда, договора найма жилого помещения муниципального специализированного жилищного фонда или ордера на жилое помещение-->


				<xsd:simpleType name="DUBLICATE_DOCUMENT_TYPE">
					<xsd:annotation>
						<xsd:documentation>Тип документа для которого необходимо сделать дубликат</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Договора социального найма жилого помещения муниципального жилищного фонда</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Договора найма жилого помещения муниципального специализированного жилищного фонда</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Ордера на жилое помещение, занимаемого на условиях социального найма</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="4">
							<xsd:annotation>
								<xsd:documentation>Ордера на служебное жилое помещение</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>


				<xsd:complexType name = "SocialEmploymentContractDuplicateDataType">
					<xsd:annotation>
						<xsd:documentation>Инфомрация необходимая для получения дубликата договора социального найма</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="DublicateDocumentType" type="DUBLICATE_DOCUMENT_TYPE">
						<xsd:annotation>
							<xsd:documentation>Наименование документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="DublicateReceivingReason" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Причина получения дубликата</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="LivingSpaceAdress" type="AdressDataType">
						<xsd:annotation>
							<xsd:documentation>*Адрес жилого помещения, в отношении которого истребуется дубликат</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:complexType>



				<!--Услуга:Предоставление жилого помещения муниципального жилищного фонда по договору социального найма, изменение договора социального найма жилого помещения и его расторжение-->

				<xsd:simpleType name="APPLICIANT_SOCIAL_CATEGORY_FOR_LEASE_TYPE">
					<xsd:annotation>
						<xsd:documentation>Тип категории к которой относится заявитель</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>ветераны боевых действий в Афганистане и военнослужащие, уволенные в запас, принятые на учет в качестве нуждающихся в улучшении жилищных условий до 01.03.2005</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>граждане, принятые на учет в качестве нуждающихся в улучшении жилищных условий до 01.03.2005 (кроме ветеранов боевых действий в Афганистане и военнослужащих, уволенных в запас)</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>малоимущие граждане, принятые на учет в качестве нуждающихся в жилых помещениях, предоставляемых по договорам социального найма</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="4">
							<xsd:annotation>
								<xsd:documentation>граждане, принятые на учет для целей предоставления жилых помещений государственного жилищного фонда социального использования</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="5">
							<xsd:annotation>
								<xsd:documentation>граждане, не состоящие на учете в качестве нуждающихся в жилых помещениях, но в отношении которых вступили в законную силу решения судов о предоставлении им жилых помещений по договорам социального найма</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="6">
							<xsd:annotation>
								<xsd:documentation>подлежащие отселению наниматели жилых помещений по договорам социального найма в домах, подлежащих сносу</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="7">
							<xsd:annotation>
								<xsd:documentation>наниматели (собственники) жилых помещений в коммунальных квартирах, в которых освободилось жилое помещение муниципального жилищного фонда, являющиеся малоимущими и нуждающимися в жилых помещениях или обеспеченные общей площадью жилого помещения менее нормы предоставления</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="8">
							<xsd:annotation>
								<xsd:documentation>граждане занимающие жилые помещения в домах, исключенных из числа общежитий</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:complexType name = "SocialLeaseAgreementAmendmentDataType">
					<xsd:annotation>
						<xsd:documentation>Инфомрация необходимая для изменения договора социального найма жилого помещения</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="AppliciantSocialCategoryType" type="APPLICIANT_SOCIAL_CATEGORY_FOR_LEASE_TYPE">
						<xsd:annotation>
							<xsd:documentation>Категория к которой относится заявитель</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="SocialLeaseResidentialPremisesAdress" type="AdressDataType">
						<xsd:annotation>
							<xsd:documentation>Адрес предоставляемого жилого помещения</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="LivingSpaceAdress" type="AdressDataType">
						<xsd:annotation>
							<xsd:documentation>Адрес жилого помещения, в отношении которого истребуется дубликат</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="СohabitingFamilyMembers" type="TOGGLE_TYPE">
						<xsd:annotation>
							<xsd:documentation>Есть совместно проживающие члены семьи?</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="FamilyCompositionData" type="FamilyCompositionDataType">
						<xsd:annotation>
							<xsd:documentation>Состав семьи заявителя</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:complexType>


				<xsd:simpleType name="APPLICIANT_SOCIAL_CATEGORY_1_TYPE">
					<xsd:annotation>
						<xsd:documentation>Тип категории к которой относится заявитель</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>ветераны боевых действий в Афганистане и военнослужащие, уволенные в запас, принятые на учет в качестве нуждающихся в улучшении жилищных условий до 01.03.2005</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>граждане, принятые на учет в качестве нуждающихся в улучшении жилищных условий до 01.03.2005 (кроме ветеранов боевых действий в Афганистане и военнослужащих, уволенных в запас)</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>малоимущие граждане, принятые на учет в качестве нуждающихся в жилых помещениях, предоставляемых по договорам социального найма</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="4">
							<xsd:annotation>
								<xsd:documentation>граждане, принятые на учет для целей предоставления жилых помещений государственного жилищного фонда социального использования</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="5">
							<xsd:annotation>
								<xsd:documentation>граждане, не состоящие на учете в качестве нуждающихся в жилых помещениях, но в отношении которых вступили в законную силу решения судов о предоставлении им жилых помещений по договорам социального найма</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="6">
							<xsd:annotation>
								<xsd:documentation>подлежащие отселению наниматели жилых помещений по договорам социального найма в домах, подлежащих сносу</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="7">
							<xsd:annotation>
								<xsd:documentation>наниматели (собственники) жилых помещений в коммунальных квартирах, в которых освободилось жилое помещение муниципального жилищного фонда, являющиеся малоимущими и нуждающимися в жилых помещениях или обеспеченные общей площадью жилого помещения менее нормы предоставления</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="8">
							<xsd:annotation>
								<xsd:documentation>граждане занимающие жилые помещения в домах, исключенных из числа общежитий</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>





				<xsd:complexType name = "SocialLeaseAgreementAmendmentDataType">
					<xsd:annotation>
						<xsd:documentation>Инфомрация необходимая для изменения договора социального найма жилого помещения</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="AppliciantSocialCategoryType" type="APPLICIANT_SOCIAL_CATEGORY_1_TYPE">
						<xsd:annotation>
							<xsd:documentation>Категория к которой относится заявитель</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="SocialLeaseResidentialPremisesAdress" type="AdressDataType">
						<xsd:annotation>
							<xsd:documentation>Адрес предоставляемого жилого помещения</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="LivingSpaceAdress" type="AdressDataType">
						<xsd:annotation>
							<xsd:documentation>Адрес жилого помещения, в отношении которого истребуется дубликат</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="СohabitingFamilyMembers" type="TOGGLE_TYPE">
						<xsd:annotation>
							<xsd:documentation>Есть совместно проживающие члены семьи?</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="FamilyCompositionData" type="FamilyCompositionDataType">
						<xsd:annotation>
							<xsd:documentation>Состав семьи заявителя</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:complexType>



				<xsd:simpleType name="APPLICIANT_SOCIAL_CATEGORY_2_TYPE">
					<xsd:annotation>
						<xsd:documentation>Тип категории к которой относится заявитель</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">

						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>граждане, состоящие на учете в качестве нуждающихся в жилых помещениях, предоставляемых по договорам социального найма</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>

						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>граждане, получившие или перенесшие лучевую болезнь и другие заболевания, связанные с радиационным воздействием вследствие чернобыльской катастрофы или с работами по ликвидации последствий катастрофы на Чернобыльской АЭС (при условии признания их нуждающимися в улучшении жилищных условий)</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>инвалиды вследствие чернобыльской катастрофы (при условии признания их нуждающимися в улучшении жилищных условий)</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="4">
							<xsd:annotation>
								<xsd:documentation>граждане, являющиеся ветеранами боевых действий на территории СССР, на территории Российской Федерации и территориях других государств</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="5">
							<xsd:annotation>
								<xsd:documentation>граждане из подразделений особого риска в пределах, установленных Постановлением Верховного Совета РФ от 27.12.1991 г. N 2123-1 "О распространении действия Закона РСФСР "О социальной защите граждан, подвергшихся воздействию радиации вследствие катастрофы на Чернобыльской АЭС" на гр. из подразделений особого риска</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="6">
							<xsd:annotation>
								<xsd:documentation>граждане, эвакуированные (в т.ч. выехавшие добровольно) в 1986 году из зоны отчуждения или переселенные, в т.ч. выехавшие добровольно, из зоны отселения в 1986 году и в посл. годы, включая детей, в т.ч. детей, которые в момент эвакуации находились(находятся) в состоянии внутриутробного развития</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="7">
							<xsd:annotation>
								<xsd:documentation>граждане, подвергшиеся воздействию радиации вследствие аварии в 1957 году на производственном объединении "Маяк" и сбросов радиоактивных отходов в реку Теча</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>

						<xsd:enumeration value="8">
							<xsd:annotation>
								<xsd:documentation>граждане, являющиеся родителями или лицами, их заменяющими, воспитывающие трех и более несовершеннолетних детей и совместно обратившиеся за предоставлением земельного участка</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="9">
							<xsd:annotation>
								<xsd:documentation>Герои Социалистического Труда и полные кавалеры орденов Трудовой Славы в соответствии с пунктом 4 статьи 3 Федерального закона от 9 января 1997 года N 5-ФЗ "О предоставлении социальных гарантий Героям Социалистического Труда и полным кавалерам орденов Трудовой Славы"</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="10">
							<xsd:annotation>
								<xsd:documentation>инвалиды и семьи, имеющие в своем составе инвалидов</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="11">
							<xsd:annotation>
								<xsd:documentation>граждане, окончившие образовательные учреждения начального, среднего и (или) высшего профессионального образования и работающие по трудовому договору в сельской местности по полученной специальности</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="12">
							<xsd:annotation>
								<xsd:documentation>граждане, не достигшие возраста 35 лет, состоящие между собой в браке и совместно обратившиеся за предоставлением земельного участка</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="13">
							<xsd:annotation>
								<xsd:documentation>Герои Советского Союза, Герои Российской Федерации, полные кавалеры ордена Славы в соответствии с пунктом 4 статьи 5 Закона Российской Федерации от 15 января 1993 года N 4301-1 "О статусе Героев Советского Союза, Героев Российской Федерации и полных кавалеров орденов Славы</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="14">
							<xsd:annotation>
								<xsd:documentation>граждане, получившие суммарную (накопленную) эффективную дозу облучения, превышающую 25 сЗв (бэр) (при условии признания их нуждающимися в улучшении жилищных условий)</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="15">
							<xsd:annotation>
								<xsd:documentation>военнослужащие (в соотвествии с пунктом 12 статьи 15 Федерального закона от 27 мая 1998 года N 76-ФЗ "О статусе военнослужащих")</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="16">
							<xsd:annotation>
								<xsd:documentation>граждане, являющиеся одинокими родителями или лицами, их заменяющими, воспитывающие несовершеннолетних детей</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:complexType name = "LandPlotsOnetimeProvisionDataType">
					<xsd:annotation>
						<xsd:documentation>Предоставление однократно бесплатно в собственность граждан земельных участков</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="AppliciantSocialCategoryType" type="APPLICIANT_SOCIAL_CATEGORY_2_TYPE">
						<xsd:annotation>
							<xsd:documentation>Категория к которой относится заявитель</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

				</xsd:complexType>





				<!--Услуга: Принятие малоимущих граждан на учет в качестве нуждающихся в жилых помещениях, предоставляемых по договорам социального найма-->
				<!--Услуга: Принятие граждан на учет для целей предоставления жилых помещений в государственном жилищном фонде социального использования Свердловской области-->

				<xsd:simpleType name="RESIDENTIAL_PREMISES_NEED_REASON_TYPE">
					<xsd:annotation>
						<xsd:documentation>Типы оснований для признания нуждающимся в жилых помещениях</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>обеспеченность общей площадью жилого помещения на одного члена семьи менее учетной нормы</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>отсутствие жилого помещения по договору социального найма или на праве собственности</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>проживание в жилом помещении, занятом несколькими семьями, если в составе семьи имеется больной, страдающий тяжелой формой хронического заболевания, при которой совместное проживание с ним в одной квартире невозможно,  при отсутствии иного жилого помещения по договору социального найма или на праве собственности</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="4">
							<xsd:annotation>
								<xsd:documentation>проживание в помещении, не отвечающем установленным для жилых помещений требованиям</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:simpleType name="RESIDENTIAL_PREMISES_TYPE">
					<xsd:annotation>
						<xsd:documentation>Типы жилых помещений</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Жилой дом</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Часть жилого дома</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Квартира</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="4">
							<xsd:annotation>
								<xsd:documentation>Часть квартиры</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="5">
							<xsd:annotation>
								<xsd:documentation>Комната</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:simpleType name="RESIDENTIAL_PREMISES_OWNERSHIP_TYPE">
					<xsd:annotation>
						<xsd:documentation>Виды собственности</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Переданная в доверительное управление</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Долевая</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Единоличная</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="4">
							<xsd:annotation>
								<xsd:documentation>Совместная</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>




				<xsd:complexType name = "ResidentialPremisesCitizensRegistrationDataType">
					<xsd:annotation>
						<xsd:documentation>Информация для постановки граждан на учёт в качестве нуждающихся в жилом помещении</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="ResidentialPremisesCitizensRegistrationReason" type="RESIDENTIAL_PREMISES_NEED_REASON_TYPE" maxOccurs="4">
						<xsd:annotation>
							<xsd:documentation>Основание для признания нуждающимся в жилых помещениях</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="CitizenResidentialPremisesInformation" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Сведения о жилых помещениях, занимаемых заявителями, совместно проживающими с ними членами семьи, их супругами</xsd:documentation>
						</xsd:annotation>

						<xsd:element name="CitizenFullname" type="xsd:string" maxOccurs="4">
							<xsd:annotation>
								<xsd:documentation>Фамилия, имя, отчество заявителя (члена его семьи) либо гражданина, признанного недееспособным</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ResidentialPremisesType" type="RESIDENTIAL_PREMISES_TYPE">
							<xsd:annotation>
								<xsd:documentation>Вид жилого помещения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ResidentialPremisesOwnershipType" type="RESIDENTIAL_PREMISES_OWNERSHIP_TYPE">
							<xsd:annotation>
								<xsd:documentation>Вид собственности</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ResidentialPremisesAdress" type="AdressDataType">
							<xsd:annotation>
								<xsd:documentation>Адрес жилого помещения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ResidentialPremisesArea" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Общая площадь жилого помещения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="TitleDocumentsDetails" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Реквизиты правоустанавливающего документа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

					<xsd:element name="ApplicantProppertyData">

						<xsd:element name="IsApplicantIsTenant" type="TOGGLE_TYPE">
							<xsd:annotation>
								<xsd:documentation>Являются ли заявитель и члены семьи заявителя нанимателем жилых помещений по договорам социального найма</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<xsd:element name="ApplicantIsTenant" minOccurs="0" maxOccurs="unbounded">
							<xsd:element name="TitleDocumentsDetails" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Реквизиты правоустанавливающего документа</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="ResidentialPremisesAdress" type="AdressDataType">
								<xsd:annotation>
									<xsd:documentation>Адрес жилого помещения</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ResidentialPremisesTenant" type="AdressDataType">
								<xsd:annotation>
									<xsd:documentation>Наниматель жилого помещения</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:element>

						<xsd:element name="IsApplicantMakesDeteriorationHousingConditionsMovements" type="TOGGLE_TYPE">
							<xsd:annotation>
								<xsd:documentation>Совершали ли намеренно заявитель и члены семьи заявителя в течение 5 лет, предшествующих дню подачи заявления о принятии на учет, действия, приведшие к ухудшению жилищных условий, а именно: к уменьшению размера занимаемого жилого помещения либо к отчуждению жилых помещений</xsd:documentation>
							</xsd:annotation>
						</xsd:element>


						<xsd:element name="ApplicantMakesDeteriorationHousingConditionsMovements" minOccurs="0" maxOccurs="unbounded">

							<xsd:element name="ParticipantsFullName" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Ф. И. О. совершавших действия</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="MovementsType" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>*Какие действия совершались</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ResidentialPremisesTenant" type="date">
								<xsd:annotation>
									<xsd:documentation>Дата</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:element>


						<xsd:element name="IsApplicantHaveRealEstateObject" type="TOGGLE_TYPE">
							<xsd:annotation>
								<xsd:documentation>Находятся ли в собственности заявителя (или членов семьи заявителя) объекты недвижимости, подлежащие налогообложению налогом на имущество физических лиц (дачи, гаражи, иные строения, помещения, сооружения)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>


						<xsd:element name="ApplicantHaveRealEstateObject" minOccurs="0" maxOccurs="unbounded">

							<xsd:element name="RealEstateObjects" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Объекты недвижимости</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="RealEstateObjectsCost" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Стоимость имущества, исчисленная для налоговой базы на имущество физических лиц</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

						</xsd:element>


						<xsd:element name="IsApplicantHaveLandPlot" type="TOGGLE_TYPE">
							<xsd:annotation>
								<xsd:documentation>Находятся ли в собственности заявителя (или членов семьи заявителя) земельные участки, относящиеся к объекту налогообложения земельным налогом</xsd:documentation>
							</xsd:annotation>
						</xsd:element>


						<xsd:element name="ApplicantHaveLandPlot" minOccurs="0" maxOccurs="unbounded">

							<xsd:element name="LandPlots" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Земельные участки</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="LandPlotsCost" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Стоимость имущества, исчисленная для налоговой базы по земельному налогу, составляет</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

						</xsd:element>


						<xsd:element name="IsApplicantHaveTransport" type="TOGGLE_TYPE">
							<xsd:annotation>
								<xsd:documentation>Находятся ли в собственности заявителя (или членов семьи заявителя) транспортные средства, относящиеся к объекту налогообложения транспортным налогом</xsd:documentation>
							</xsd:annotation>
						</xsd:element>


						<xsd:element name="ApplicantHaveTransport" minOccurs="0" maxOccurs="unbounded">

							<xsd:element name="Transport" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Транспортные средства</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="TransportCost" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Рыночная стоимость транспортного средства на дату подачи заявления составляет</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

						</xsd:element>

					</xsd:element>


					<xsd:element name="FamilyCompositionData" type="FamilyCompositionDataType">
						<xsd:annotation>
							<xsd:documentation>Состав семьи заявителя</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

				</xsd:complexType>









				<!--Услуга: Включение заявителей в список граждан, имеющих право быть принятыми в члены жилищно-строительного кооператива, создаваемого в целях обеспечения жильем граждан в соответствии с Федеральным законом от 24.07.2008 № 161-ФЗ «О содействии развитию жилищного строительства»-->

				<xsd:simpleType name="LIVING_HOUSE_TYPE">
					<xsd:annotation>
						<xsd:documentation>Типы жилых помещений</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Многоквартирный дом</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Малоэтажный индивидуальный жилой дом</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Малоэтажный жилой дом блокированной застройки</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>


				<xsd:complexType name = "HousingConstructionCooperativeAcceptanceDataType">
					<xsd:annotation>
						<xsd:documentation>Данные необходимые для включения заявителей в члены жилищно строительного кооператива</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="ApplicantWorkPlase" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Место работы</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ApplicantWorkPlasePosition" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Должность</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ApplicantWorkExpirience" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Стаж</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="FamilyCompositionData" type="FamilyCompositionDataType">
						<xsd:annotation>
							<xsd:documentation>Состав семьи заявителя</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="LivingHouseType" type="LIVING_HOUSE_TYPE">
						<xsd:annotation>
							<xsd:documentation>Тип жилого помещения</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="LivingHouseArea" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Общая площадь жилого помещения</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="LivingHouseRoomsAmount" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Количество комнат</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

				</xsd:complexType>



				<!--Услуги: Выдача специального разрешения на автомобильные перевозки тяжеловесных и (или) крупногабаритных грузов по маршрутам, проходящим полностью или частично по автомобильным дорогам местного значения в границах муниципального образования «город Екатеринбург»-->


				<xsd:complexType name = "CargoTransportSpecialPermitIssuanceDataType">
					<xsd:annotation>
						<xsd:documentation>Данные необходимые для выдачи специального разрещения на перевозки товаров</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="TransportOwner" type="RecipientPersonalDataType">
						<xsd:annotation>
							<xsd:documentation>Владелец транспортного средства</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="RouteInformation">
						<xsd:annotation>
							<xsd:documentation>Сведения о маршруте</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="Route" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Маршрут движения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="TransportationPeriodFrom" type="date">
							<xsd:annotation>
								<xsd:documentation>Срок перевозки, с</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="TransportationPeriodTo" type="date">
							<xsd:annotation>
								<xsd:documentation>Срок перевозки, по</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="TripsNumber" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Количество поездок</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

					</xsd:element>

					<xsd:element name="CargoInformation">
						<xsd:annotation>
							<xsd:documentation>Сведения о грузе</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="IsDivisible" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>Является ли груз делимым</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="CargoName" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Наименование груза</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="OverallDimensions">
							<xsd:annotation>
								<xsd:documentation>Габариты груза</xsd:documentation>
							</xsd:annotation>
							<xsd:element name="Length" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Длина</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Width" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Ширина</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Height" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Высота</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Weight" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Масса</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:element>


					</xsd:element>


				</xsd:complexType>


				<!--Услуга: Прием заявлений и организация предоставления гражданам субсидий на оплату жилых помещений и коммунальных услуг-->



				<xsd:simpleType name="PREFERENTIAL_CATEGORY_TYPE">
					<xsd:annotation>
						<xsd:documentation>Типы льготной категории</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Бывшие несовершеннолетние узники концлагерей</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Бывшие несовершеннолетние узники концлагерей-инвалиды</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Бывшие совершеннолетние узники нацистских концлагерей</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="4">
							<xsd:annotation>
								<xsd:documentation>Бюджетные работники сельской местности </xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="5">
							<xsd:annotation>
								<xsd:documentation>Ветераны боевых действий</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="6">
							<xsd:annotation>
								<xsd:documentation>Ветераны труда и лица, приравненные к ним по состоянию на 31.12.2004 </xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="7">
							<xsd:annotation>
								<xsd:documentation>Военнослужащие и лица рядового и начальствующего состава ОВД, Государственной противопожарной службы, учреждений и органов уголовно-исполнительной системы, ставшие инвалидами вследствие ранения, контузии или увечья, полученных при исполнении обязанностей военной службы (служебных обязанностей)</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="8">
							<xsd:annotation>
								<xsd:documentation>Герои Российской Федерации</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="9">
							<xsd:annotation>
								<xsd:documentation>Герои Советского Союза</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="10">
							<xsd:annotation>
								<xsd:documentation>Герои Социалистического Труда</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="11">
							<xsd:annotation>
								<xsd:documentation>Граждане из подразделений особого риска</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="12">
							<xsd:annotation>
								<xsd:documentation>Граждане, подвергшиеся воздействию радиации вследствие аварии в 1957 году на ПО Маяк и сбросов радиоактивных отходов в реку Теча</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="13">
							<xsd:annotation>
								<xsd:documentation>Граждане, подвергшиеся радиационному воздействию вследствие Чернобыльской катастрофы</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="14">
							<xsd:annotation>
								<xsd:documentation>Граждане, подвергшиеся радиационному воздействию вследствие ядерных испытаний на Семипалатинском полигоне</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="15">
							<xsd:annotation>
								<xsd:documentation>Граждане, получившие увечье или заболевание, не повлекшие инвалидности, при прохождении военной службы или службы в ОВД РФ в период действия чрезвычайного положения либо вооруженного конфликта</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="16">
							<xsd:annotation>
								<xsd:documentation>Инвалиды войны</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="17">
							<xsd:annotation>
								<xsd:documentation>Инвалиды, в том числе ВИЧ-инфицированные - несовершеннолетние в возрасте до 18 лет</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="18">
							<xsd:annotation>
								<xsd:documentation>Лица, награжденные знаком "Жителю блокадного Ленинграда", имеющие инвалидность</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="19">
							<xsd:annotation>
								<xsd:documentation>Лица, награжденные знаком "Жителю блокадного Ленинграда", не имеющие инвалидность</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="20">
							<xsd:annotation>
								<xsd:documentation>Лица, награжденные знаком отличия Свердловской области "За заслуги перед Свердловской областью" 1 степени</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="21">
							<xsd:annotation>
								<xsd:documentation>Лица, признанные пострадавшими от политических репрессий</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="22">
							<xsd:annotation>
								<xsd:documentation>Лица, проработавшие в тылу в период с 22 июня 1941 года по 9 мая 1945 года не менее шести месяцев, исключая период работы на временно оккупированных территориях СССР; награжденные орденами или медалями СССР за самоотверженный труд в период Великой Отечественной войны</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="23">
							<xsd:annotation>
								<xsd:documentation>Лица, удостоенные звания "Почетный гражданин Свердловской области"</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="24">
							<xsd:annotation>
								<xsd:documentation>Многодетные семьи</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="25">
							<xsd:annotation>
								<xsd:documentation>Полные кавалеры ордена Славы</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="26">
							<xsd:annotation>
								<xsd:documentation>Полные кавалеры ордена Трудовой Славы</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="27">
							<xsd:annotation>
								<xsd:documentation>Реабилитированные лица</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="28">
							<xsd:annotation>
								<xsd:documentation>Семьи, имеющие детей-инвалидов</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="29">
							<xsd:annotation>
								<xsd:documentation>Семьи, потерявшие кормильца из числа граждан, пострадавших вследствии воздействия радиации</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="30">
							<xsd:annotation>
								<xsd:documentation>Участники Великой Отечественной войны</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="31">
							<xsd:annotation>
								<xsd:documentation>Участники Великой Отечественной войны, ставшие инвалидами</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="32">
							<xsd:annotation>
								<xsd:documentation>Члены семей погибших(умерших) инвалидов войны, участников ВОВ, ветеранов боевых  действий, военнослужащих</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>


				<xsd:complexType name="IncomeInformationDataType">
					<xsd:annotation>
						<xsd:documentation>Сведения о доходах</xsd:documentation>
					</xsd:annotation>
					<xsd:element name="IncomeType" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Вид дохода</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="IncomeDetails" maxOccurs="undounded">
						<xsd:annotation>
							<xsd:documentation>Информация о доходе</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="Month" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Месяц</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Year" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Год</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Amount" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Размер в рублях</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>
				</xsd:complexType>

				<xsd:simpleType name="RECEIVING_PAYMENTS_METHOD_TYPE">
					<xsd:annotation>
						<xsd:documentation>Способ получения выплат</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Банковский перевод</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Почтовый перевод</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Иная доставочная организация</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>



				<xsd:complexType name = "ResidentialPremisesPaymentSubsidiesDataType">
					<xsd:annotation>
						<xsd:documentation>Данные необходимые для для предоставления государственной услуги, их первичная проверка и регистрация</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="PreferentialCategoryType" type="PREFERENTIAL_CATEGORY_TYPE" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Наименование льготной категории</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="AdditionalInformation" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Дополнительная информация</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="IncomeInformationData" type="IncomeInformationDataType">
						<xsd:annotation>
							<xsd:documentation>Сведения о доходах</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="IncomeInformationData" type="RECEIVING_PAYMENTS_METHOD_TYPE">
						<xsd:annotation>
							<xsd:documentation>Метод получения субсидий</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="FamilyCompositionData" type="FamilyCompositionDataType">
						<xsd:annotation>
							<xsd:documentation>Состав семьи заявителя</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="FamilyMembersIncomeInformation">
						<xsd:annotation>
							<xsd:documentation>Сведения о доходах членов семьи</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="FamilyMemberFIO" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Член семьи</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="FamilyMemberIncomeInformationData" type="IncomeInformationDataType">
							<xsd:annotation>
								<xsd:documentation>Сведения о доходах</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

					<xsd:element name="IsHasDebt" type="CHECKBOX_TYPE">
						<xsd:annotation>
							<xsd:documentation>Имеет ли заявитель задолженность по оплате</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DebtAmount" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Задолженность по оплате</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

				</xsd:complexType>


				<!--Услуга: Выдача разрешения на вступление в брак лицам, достигшим возраста шестнадцати лет-->

				<xsd:complexType name = "MarriageLicenseDataType">
					<xsd:annotation>
						<xsd:documentation>Данные необходимые для разрешения на вступление в брак</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="MarriagePartner" type="PersonDataType">
						<xsd:annotation>
							<xsd:documentation>Человек, с которым запрашивается разрешение на вступление в брак</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="IsPartnersBothUnderage" type="CHECKBOX_TYPE">
						<xsd:annotation>
							<xsd:documentation>Являются ли оба брачующиеся несовершеннолетними гражданами</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="MarrigeReason" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Причина вступления в брак</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:complexType>

				<!--Услуга: Выдача справки о периоде выплаты компенсации расходов на оплату жилого помещения и коммунальных услуг и ее размере-->

				<xsd:complexType name = "CompensationForExpensesPaymentPeriodDataType">
					<xsd:annotation>
						<xsd:documentation>Данные необходимые для выдачи справки о периоде выплаты компенсации расходов на оплату жилого помещения и коммунальных услуг и ее размере</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="PeriodStart" type="date">
						<xsd:annotation>
							<xsd:documentation>Начало периода выплат</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="PeriodEnd" type="date">
						<xsd:annotation>
							<xsd:documentation>Конец периода выплат</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:complexType>


				<!--Услуга: Прием заявлений, постановка на учет и зачисление детей в образовательные учреждения, реализующие основную общеобразовательную программу дошкольного образования (детские сады)-->

				<xsd:simpleType name="FAMILY_CONNECTION_TYPE">
					<xsd:annotation>
						<xsd:documentation>Кем заявитель приходится ребёнку</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Мать</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Отец</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Иное</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:simpleType name="WORKING_HOURS_TYPE">
					<xsd:annotation>
						<xsd:documentation>Режим работы детского сада</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Режим кратковременного пребывания (до 5 часов в день)</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Режим сокращенного дня (8-10-часового пребывания)</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Режим полного дня (10,5-12-часового пребывания)</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="4">
							<xsd:annotation>
								<xsd:documentation>Режим продленного дня (13-14-часового пребывания)</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="5">
							<xsd:annotation>
								<xsd:documentation>Режим круглосуточного пребывания</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:simpleType name="CHILD_BENEFIT_CATEGORY_TYPE">
					<xsd:annotation>
						<xsd:documentation>Льготная группа ребёнка</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Дети сотрудников таможенных органов Российской Федерации</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Дети сотрудника полиции</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Дети, проживающие в одной семье и имеющие общее место жительства с братьями и (или) сестрами, которые уже обучаются в общеобразовательной организации, в которую подается заявление</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="4">
							<xsd:annotation>
								<xsd:documentation>Дети военнослужащих по месту жительства их семей</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="5">
							<xsd:annotation>
								<xsd:documentation>Дети сотрудников органов федеральной противопожарной службы Государственной противопожарной службы</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="6">
							<xsd:annotation>
								<xsd:documentation>Без льгот</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="7">
							<xsd:annotation>
								<xsd:documentation>Дети сотрудников, имеющих специальные звания и проходящих службу в учреждениях и органах уголовно-исполнительной системы, органах принудительного исполнения РФ</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:complexType name = "EducationalInstitutionChildrenEnrollmentDataType">
					<xsd:annotation>
						<xsd:documentation>Данные необходимые для зачисления ребёнка в образовательное учреждение</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="FamilyConnectionType" type="FAMILY_CONNECTION_TYPE" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Кем заявитель приходится для ребёнка</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="AnotherConnectionType" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Название иного типа связи с ребёнком</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="AnotherConnectionTypeDocument" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>На основании какого документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="SecondParentPersonalData" type="PersonDataType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Персональные данные второго родителя</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="ChildPersonalInformation" type="PersonDataType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Персональные данные ребёнка</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="ChildBenefitCategoryType" type="CHILD_BENEFIT_CATEGORY_TYPE" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Льготная категория ребёнка</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="AdressOfStayMatchesToAdressOfResidence" type="CHECKBOX_TYPE" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Адрес места пребывания совпадает с местом жительства?</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="ChildAdressOfStay" type="AdressDataType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Адрес места пребывания</xsd:documentation>
						</xsd:annotation>
					</xsd:element>



					<xsd:choice>
						<xsd:element name="KindergardenEnrollment">
							<xsd:annotation>
								<xsd:documentation>Зачисление в детский сад</xsd:documentation>
							</xsd:annotation>

							<xsd:element name="SelectedInstitution" type="xsd:string" maxOccurs="3">
								<xsd:annotation>
									<xsd:documentation>Выбранное место обучения</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="SuggestAnother" type="CHECKBOX_TYPE">
								<xsd:annotation>
									<xsd:documentation>Если на момент желаемой даты зачисления в выбранные сады не будет места, прошу предложить мне другой доступный детский сад</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="EnrollmentDate" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Специфика группы</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="GroupSpecifics" type="date">
								<xsd:annotation>
									<xsd:documentation>Выбранная дата зачисления</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="GroupSpecificsDocument" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Реквизиты документа, подтверждающего необходимость в указанной специфике</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="WorkingHours" type="WORKING_HOURS_TYPE" maxOccurs="5">
								<xsd:annotation>
									<xsd:documentation>Режим работы детского сада</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="AdditionalBenefits" type="xsd:string" minOccurs="0" maxOccurs="38">
								<xsd:annotation>
									<xsd:documentation>Дополнительные льготы</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="AdditionalBenefitsDocument" minOccurs="0" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Реквизиты документа, подтверждающего наличие льгот</xsd:documentation>
								</xsd:annotation>
							</xsd:element>


						</xsd:element>
						<xsd:element name="SchoolEnrollment">
							<xsd:annotation>
								<xsd:documentation>Зачисление в школу</xsd:documentation>
							</xsd:annotation>

							<xsd:element name="ClassNumber" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Номер выбранного класса</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="SelectedInstitution" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Выбранное место обучения</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

						</xsd:element>

						<xsd:element name="FirstClassSchoolEnrollment">
							<xsd:annotation>
								<xsd:documentation>Зачисление в школу</xsd:documentation>
							</xsd:annotation>

							<xsd:element name="IsChildDisabled" type="CHECKBOX_TYPE">
								<xsd:annotation>
									<xsd:documentation>Ребенок-инвалид</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="IsChildWithDisabilities" type="CHECKBOX_TYPE">
								<xsd:annotation>
									<xsd:documentation>Ребенок с ограниченными возможностями здоровья</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="LanguageOfEducation" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Язык образования</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="SelectedInstitution" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Выбранное место обучения</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

						</xsd:element>

						<xsd:element name="AdditionalEducationInstitutionEnrollment">
							<xsd:annotation>
								<xsd:documentation>Зачисление в учреждение дополнительного образования</xsd:documentation>
							</xsd:annotation>


							<xsd:element name="AdditionalEducationDirection" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Направление дополнительного образования</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="SelectedInstitution" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Выбранное место обучения</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="EducationalProgram" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Образовательная программа</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="EducationalSubProgram" type="xsd:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Образовательная подпрограмма</xsd:documentation>
								</xsd:annotation>
							</xsd:element>


						</xsd:element>

					</xsd:choice>

				</xsd:complexType>

				<!--Услуга: Предоставление путевок для летнего отдыха детей в лагерях, организованных образовательными организациями, осуществляющими организацию отдыха и оздоровления обучающихся в каникулярное время (с круглосуточным или дневным пребыванием), санаториях и санаторно-оздоровительных лагерях круглогодичного действия, в загородных стационарных оздоровительных лагерях, специализированных (профильных) лагерях, детских лагерях различной тематической направленности -->
				<xsd:simpleType name="CHILDREN_TRIP_TYPE">
					<xsd:annotation>
						<xsd:documentation>Режим работы детского сада</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Загородные оздоровительные лагеря</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Лагеря с дневным пребыванием детей</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Санатории</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:complexType name = "ChildrenSummerHolidaysTripDataType">
					<xsd:annotation>
						<xsd:documentation>Предоставление путевок для летнего отдыха детей в лагерях</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="TripType" type="CHILDREN_TRIP_TYPE">
						<xsd:annotation>
							<xsd:documentation>Тип путёвки</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="HealthImprovingInstitutionDetails" maxOccurs="3">
						<xsd:element name="HealthImprovingInstitution" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Желаемое оздоровительное учреждение</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ChosenShift" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Выбранная смена</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>
					<xsd:element name="ChildInformation" maxOccurs="unbounded">
						<xsd:element name="ChildPersonalInformation" type="PersonDataType">
							<xsd:annotation>
								<xsd:documentation>Персональные данные ребёнка</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ChildRegistrationAdress" type="PerAdressDataType">
							<xsd:annotation>
								<xsd:documentation>Адрес регистрации</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<xsd:element name="ChildRegistrationTown" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>Ребенок проживает на территории муниципального образования «город Екатеринбург»</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ChildEducationTown" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>Ребенок получает общее образование в образовательном учреждении, расположенном на территории муниципального образования «город Екатеринбург»</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

				</xsd:complexType>


				<!--Предоставление информации об образовательных программах и учебных планах, рабочих программах учебных курсов, предметов, дисциплин (модулей), годовых календарных учебных графиках-->
				<xsd:complexType name = "EducationalInformationProvidingDataType">
					<xsd:annotation>
						<xsd:documentation>Предоставление образовательной информации</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="RequestedInformation">
						<xsd:annotation>
							<xsd:documentation>Интересующая информация</xsd:documentation>
						</xsd:annotation>

						<xsd:element name="CurriculumInformation" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>Общие сведения об образовательном учреждении</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ChildRegistrationTown" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>Получение информации об учебных программах</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ImplementedEducationalProgramsInformation" type="CHECKBOX_TYPE">
							<xsd:annotation>
								<xsd:documentation>Информация о реализуемых образовательных программах</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

					<xsd:element name="SelectedClass" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Выбранная параллель</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="EducationalInstitution" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Выбранное образовательное учреждение</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

				</xsd:complexType>



				<!--Услуга: Выдача выписки из похозяйственной книги-->

				<xsd:simpleType name="HOUSEHOLD_INFORMATION_TYPE">
					<xsd:annotation>
						<xsd:documentation>Сведения из похозяйственной книги</xsd:documentation>
					</xsd:annotation>
					<xsd:restriction base="xsd:byte">
						<xsd:enumeration value="1">
							<xsd:annotation>
								<xsd:documentation>Право на земельный участок</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="2">
							<xsd:annotation>
								<xsd:documentation>Наличие скота</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="3">
							<xsd:annotation>
								<xsd:documentation>Другое</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:complexType name = "HouseholdBookExtractDataType">
					<xsd:annotation>
						<xsd:documentation>Выдача выписки из похозяйственной книги</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="HouseholdInformationType" type="HOUSEHOLD_INFORMATION_TYPE">
						<xsd:annotation>
							<xsd:documentation>Интересующая информация</xsd:documentation>
						</xsd:annotation>
					</xsd:element>


				</xsd:complexType>



				<!--Выдача копий зарегистрированных трудовых договоров, заключенных работниками с работодателями - физическими лицами-->


				<xsd:complexType name = "EmploymentContractsCopiesIssueDataType">
					<xsd:annotation>
						<xsd:documentation>Выдача копий зарегистрированных трудовых договоров, заключенных работниками с работодателями - физическими лицами</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="EmployeeFIO" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>ФИО работника</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="EmployerFIO" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>ФИО работодателя</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="EmploymentContractsReceivingReason" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Цель получения копии трудового договора</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="EmploymentContractsOriginalMissingReason" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Причина отсутствия у заявителя подлинника запрашиваемого документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="CopiedAmount" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Количество копий</xsd:documentation>
						</xsd:annotation>
					</xsd:element>


				</xsd:complexType>

				<!--Выдача справки о захоронении-->
				<xsd:complexType name = "BurialCertificateIssuanceDataType">
					<xsd:annotation>
						<xsd:documentation>Выдача справки о захоронении</xsd:documentation>
					</xsd:annotation>

					<xsd:element name="DeceasedPersonInformation" type="PersonDataType">
						<xsd:annotation>
							<xsd:documentation>Информация об умершем</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="CemeteryName" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Наименование кладбища</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="SectionNumber" type="xsd:string">
						<xsd:annotation>
							<xsd:documentation>Номер секции захоронения</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="DateOfDeath" type="date">
						<xsd:annotation>
							<xsd:documentation>Дата смерти</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="BurialCarryOutPersonInformation" type="PersonDataType">
						<xsd:annotation>
							<xsd:documentation>Информация о лице, взявшем на себя обязанность осуществить погребение</xsd:documentation>
						</xsd:annotation>
					</xsd:element>


				</xsd:complexType>




				<!-- Документы -->
				<xsd:simpleType name="CadastralNumberDataType">
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="19"/>
						<xsd:pattern value="[0-9]{2}[:][0-9]{2}[:][0-9]{7}[:][0-9]{5}"/>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:simpleType name="OKTMODataType">
					<xsd:restriction base="xsd:string">
						<xsd:minLength value="1"/>
						<xsd:maxLength value="11"/>
					</xsd:restriction>
				</xsd:simpleType>
				<xsd:simpleType name="SNILSDataType">
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="14"/>
						<xsd:pattern value="[0-9]{3}[-][0-9]{3}[-][0-9]{3} [0-9]{2}"/>
					</xsd:restriction>
				</xsd:simpleType>
				<xsd:simpleType name="INNDataType">
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="10"/>
						<xsd:pattern value="[0-9]{10}"/>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:simpleType name="OGRNDataType">
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="13"/>
						<xsd:pattern value="[0-9]{13}"/>
					</xsd:restriction>
				</xsd:simpleType>
				<xsd:simpleType name="KPPDataType">
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="13"/>
						<xsd:pattern value="[0-9]{9}"/>
					</xsd:restriction>
				</xsd:simpleType>

				<xsd:complexType name="DocumentBlockType">
					<xsd:annotation>
						<xsd:documentation>Реквизиты документа(удостоверяющего личность/сертификата/и т.п.)</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element name="DocName" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Наименование документа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="DocSeries" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Серия</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="DocNumber" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Номер</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="DocDate" type="tns:date">
							<xsd:annotation>
								<xsd:documentation>Дата выдачи</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="DocIssued" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Кем выдано</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>



				<!--Банковские реквизиты>-->
				<xsd:complexType name="BankCreditBlockType">
					<xsd:annotation>
						<xsd:documentation>Укажите банковские реквизиты для перечисления выплаты</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element name="AccountHolderData" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Фамилия, имя, отчество или наименование владельца счета</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="PaymentAccount" type="xsd:string" minOccurs="0" maxOccurs="1">
							<xsd:annotation>
								<xsd:documentation>Расчётный счёт банка</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="CorrespondentAccount" type="xsd:string" minOccurs="0" maxOccurs="1">
							<xsd:annotation>
								<xsd:documentation>Корреспондентский счёт банка</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="BankBIKorName" type="xsd:string" minOccurs="0" maxOccurs="1">
							<xsd:annotation>
								<xsd:documentation>БИК или наименование банка</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="BankName" type="xsd:string" minOccurs="0" maxOccurs="1">
							<xsd:annotation>
								<xsd:documentation>Наименование банка</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="BankBIK" type="string" minOccurs="0" maxOccurs="1">
							<xsd:annotation>
								<xsd:documentation>БИК банка</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="AccountNumber" type="string" minOccurs="0" maxOccurs="1">
							<xsd:annotation>
								<xsd:documentation>Номер счета заявителя</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="CardtNumber" type="string" minOccurs="0" maxOccurs="1">
							<xsd:annotation>
								<xsd:documentation>Номер банковской карты заявителя</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="BankKPPDataType" type="string" minOccurs="0" maxOccurs="1">
							<xsd:annotation>
								<xsd:documentation>КПП</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="BankINNDataType" type="string" minOccurs="0" maxOccurs="1">
							<xsd:annotation>
								<xsd:documentation>ИНН</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>

				<xsd:complexType name="Generalize_DocumentType">
					<xsd:annotation>
						<xsd:documentation>Обобщённый тип документа</xsd:documentation>
					</xsd:annotation>
					<xsd:element name="DocumentName" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Наименование документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DocumentFile" type="DocInfoType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Документ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:complexType>

				<!--Описание вложений-->
				<xsd:complexType name="DocumentsType">
					<xsd:annotation>
						<xsd:documentation>Сведения о вложениях</xsd:documentation>
					</xsd:annotation>



					<xsd:element name="Generalize_Document" type = "Generalize_DocumentType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Документ без конкретного назначения</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="Appointment_Details" type="DocInfoType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Копия заявления</xsd:documentation>
						</xsd:annotation>
					</xsd:element>


					<xsd:element name="RecipientIdentity_Document" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Документ, удостоверяющий личность заявителя</xsd:documentation>
						</xsd:annotation>
					</xsd:element>


					<xsd:element name="EmploymentRecords_Document" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Копия трудовой книжки</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="Testament_Document" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Завещание</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="FamilyRelationshipsApproval_Document" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Копии документов, подтверждающих семейные отношения (свидетельство о браке и другие)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="ResidentPlaseApproval_Document" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Копии документов, подтверждающих место жительства</xsd:documentation>
						</xsd:annotation>
					</xsd:element>


					<xsd:element name="BirthCertificate_Document" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Свидетельство о рождении</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="PersonalDataProcessingAgreement_Document" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Согласие на обработку персональных данных</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="CourtsDecisions_Document" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Решения судов</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="LastnameChangeCertificate_Document" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Свидетельство о смене фамилии несовершеннолетнего лица, о котором запрашивается архивная информация</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="BTICertificate_Document" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Справка БТИ о принадлежности объекта недвижимости</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="NotaryRequest_Document" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Запрос нотариуса о выдаче дубликата договора приватизации</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="OwnershipCertificate_Document" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Свидетельство о праве собственности</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="MedicalInstitutions" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Документы\справки медицинских учреждений</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="MedicalInstitutions_Document" type = "Generalize_DocumentType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Документ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

					<xsd:element name="SocialInstitutions" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Документы\справки социальный учреждений подтвержающие права на социальную поддержку</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="SocialInstitutions_Document" type = "Generalize_DocumentType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Документ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

					<xsd:element name="MilitaryInstitutions" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Документы\справки предоставлемые и/или связанные с военными учреждениями</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="MilitaryInstitutions_Document" type = "Generalize_DocumentType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Документ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

					<xsd:element name="EducationInstitutions" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Документы\справки предоставлемые и/или связанные с учебными учреждениями (дипломы об образовании и т.п.)</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="EducationInstitutions_Document" type = "Generalize_DocumentType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Документ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

					<xsd:element name="PlaceOfWork" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Документы\справки предоставлемые и/или связанные с местом работы</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="PlaceOfWork_Document" type = "Generalize_DocumentType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Документ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

					<xsd:element name="ResidentialPremises" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Документы\справки предоставлемые и/или связанные с жилым(и) помещением(ями)</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="PlaceOfWork_Document" type = "Generalize_DocumentType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Документ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

					<!--Группы документов для услуг -->

					<xsd:element name="PowerOfAppointment" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Информация о копии доверенности представиеля</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="PowerOfAppointment_Details" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Реквизиты доверенности</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<xsd:element name="PowerOfAppointment_Document" type="DocInfoType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Копия доверенности представиеля</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>
					<xsd:element name="ArticlesAssociationExtract" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>Выписка из устава</xsd:documentation>
						</xsd:annotation>
					</xsd:element>

					<xsd:element name="MeteringDevices_Documents" minOccurs="0">
						<!-- Документы подтверждающие приобретение и оплату приборов учёта и комплектующих к ним-->
						<xsd:element name="MeteringDevices_Receipts" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Кассовые и товарные чеки или платежные поручения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="MeteringDevices_PurchaseAndSaleAgreement" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Договор купли-продажи (поставки) приборов учета и комплектующих к ним, материалов, необходимых для установки приборов учета</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="MeteringDevices_ConsignmentNotes" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Товарные накладные</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<!--Документы, подтверждающие выполнение и оплату работ по установке индивидуальных приборов учёта-->
						<xsd:element name="MeteringDevicesInstallation_Agreement" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Договор на выполнение работ по установке индивидуальных приборов учета</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="MeteringDevicesInstallation_Certificates" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Акты выполненных работ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="MeteringDevicesInstallation_Receipts" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Кассовые чеки или платежные поручения </xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<!-- Прочие документы и акты-->
						<xsd:element name="MeteringDevices_Passport" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Паспорта на приборы учета</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="MeteringDevices_CommissioningCertificates" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Акты ввода приборов учета в эксплуатацию</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="MeteringDevices_RegisterCertificate" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Свидетельство о внесении записи в Единый государственный реестр юридических лиц или свидетельство о внесении записи в Единый государственный реестр индивидуальных предпринимателей</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ResidentialLeaseAgreement" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Договор социального найма жилого помещения/договор найма специализированного муниципального жилищного фонда/договор найма жилого помещения/ордер на жилое помещение</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

					<!--Услуга: Выдача градостроительных планов земельных участков-->
					<xsd:element name="LandPlot_Documents" minOccurs="0">
						<!-- Документы подтверждающие приобретение и оплату приборов учёта и комплектующих к ним-->
						<xsd:element name="LandPlot_OwnershipRights" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Документ, удостоверяющий (устанавливающий) права на земельный участок</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LandPlot_BuildingOwnershipRights" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Документ, удостоверяющий (устанавливающий) права на здание, строение, сооружение, находящиеся на земельном участке</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>
					<!--Услуга: Подготовка справки о состоянии расчетов с бюджетом по арендной плате за землю-->
					<xsd:element name="LandPlotRent_Documents" minOccurs="0">
						<!-- Документы подтверждающие приобретение и оплату приборов учёта и комплектующих к ним-->
						<xsd:element name="LandPlotRent_Requisites" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Реквизиты договора на аренду</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LandPlotRent_PaymentReceipts" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Копии квитанций об уплате арендной платы</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>
					<!--Услуга: Выдача разрешений на строительство (реконструкцию), продление (прекращение) действия разрешений на строительство (реконструкцию), внесение изменений в разрешения на строительство (реконструкцию) объектов капитального строительства-->
					<xsd:element name="IssuancePermits_Documents" minOccurs="0">
						<xsd:element name="IssuancePermits_DocumentName" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Наименование документа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="IssuancePermits_DocumentId" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Номер документа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="IssuancePermits_DocumentDate" type="date" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Дата выдачи документа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LandPlot_OwnershipRights" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Документ, удостоверяющий (устанавливающий) права на земельный участок</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

					<!--Услуга: Согласование местоположения границ земельных участков, являющихся смежными по отношению к земельным участкам, находящимся в муниципальной собственности или в государственной собственности до ее разграничения-->

					<xsd:element name="CoordinationLocationLandBoundariesData_Documents" minOccurs="0">

						<xsd:element name="BoundaryPlan" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Межевой план с актом согласования местоположения границ, подготовленные в соответствии со статьями 38, 40 Федерального закона от 24.07.2007 № 221-ФЗ «О государственном кадастре недвижимости»</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LandPlotLayoutSchemes" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Копия утвержденной схемы расположения земельного участка на кадастровом плане территории</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<xsd:element name="CertificatesOwnership" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Копия свидетельства о праве собственности, праве пожизненно наследуемого владения, постоянного (бессрочного) пользования на землю</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="AdministrativeActCertifyingOwnership" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Копия государственного акта, удостоверяющего право собственности на землю, пожизненного наследуемого владения, бессрочного (постоянного) пользования землей граждан, предприятий, учреждений, организаций или крестьянских (фермерских) хозяйств</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="GrantingLandPlotDecision" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Копия документа, содержащего решение о предоставлении земельного участка на определенном праве конкретному лицу, изданного уполномоченным органом (организацией) в порядке, установленном законодательством, действовавшим в месте издания такого документа на момент его издания</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="HouseholdBookExtract" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Копия выписки из похозяйственной книги</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LandPlotLayoutSchemes" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Копия вступившего в законную силу судебного акта</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LandPlotLayoutApproval" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Решение об утверждении схемы расположения земельного участка на кадастровом плане территории</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="StateRegistrationOfRights" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Сведения о государственной регистрации прав на земельный участок</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="CadastralStatementOfEstateObject" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Сведения о земельном участке, внесенные в государственный кадастр недвижимости, в виде кадастровой выписки об объекте недвижимости, кадастрового паспорта недвижимости, кадастрового плана территории</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

					<!--Услуга: Прием в собственность муниципального образования «город Екатеринбург» имущества, находящегося в частной собственности-->
					<xsd:element name="PrivatePropertyData_Documents" minOccurs="0">

						<xsd:element name="PropertyList" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Копия пообъектного перечня имущества, принадлежащего заявителю и предлагаемого к передаче в собственность</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="PropertyRights" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Копии документов, удостоверяющих (устанавливающих) права на объект недвижимости</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="PropertyTechnicalCondition" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Копии актов технического состояния имущества</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="IndividualEntrepreneurRegistryExtract" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Выписка из Единого государственного реестра индивидуальных предпринимателей, содержащая общие сведения об индивидуальном предпринимателе</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<xsd:element name="EstateObjectRegisteredRightsExtract" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Выписка из Единого государственного реестра прав на недвижимое имущество и сделок с ним о зарегистрированных правах на объект недвижимости, предлагаемый к передаче, содержащая общедоступные сведения о зарегистрированных правах на объект недвижимости или уведомление об отсутствии в ЕГРП запрашиваемых сведений</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LandPlotRegisteredRightsExtract" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Выписка из Единого государственного реестра прав на недвижимое имущество и сделок с ним о зарегистрированных правах на объект недвижимости, предлагаемый к передаче, содержащая общедоступные сведения о зарегистрированных правах на земельный участок или уведомление об отсутствии в ЕГРП запрашиваемых сведений</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="EstateObjectCadastralPassport" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Кадастровый паспорт объекта недвижимости, предлагаемого к передаче в собственность муниципального образования</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LandPlotCadastralPassport" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Кадастровый паспорт земельного участка, предлагаемого к передаче в собственность муниципального образования</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

					<!--Услуга: Отчуждение недвижимого имущества, находящегося в муниципальной собственности и арендуемого субъектами малого и среднего предпринимательства-->
					<xsd:element name="RealEstatePropertyData_Documents" minOccurs="0">

						<xsd:element name="IndividualEntrepreneurStateRegistrationCertificate" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Свидетельство о государственной регистрации физического лица в качестве индивидуального предпринимателя</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="IndividualEntrepreneurINNCertificate" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Копия свидетельства ИНН индивидуального предпринимателя</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<xsd:element name="LegalEntityAppointmentPosition" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Копии документов о назначении (об избрании), подтверждающих полномочия действовать от имени организации без доверенности</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LegalEntityСonstituentВocuments" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Копии учредительных документов юридического лица</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<xsd:element name="LegalEntityLetter" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Копия письма юридического лица, содержащего сведения о доле Российской Федерации, субъекта Российской Федерации или муниципального образования в уставном капитале юридического лица</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="UnifiedStateRegisterRightsImmovablePropertyExtract" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Выписка из Единого государственного реестра прав на недвижимое имущество и сделок с ним о зарегистрированных правах на объект недвижимости, предлагаемый к передаче, содержащая общедоступные сведения о зарегистрированных правах на земельный участок или уведомление об отсутствии в ЕГРП запрашиваемых сведений</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

					</xsd:element>


					<xsd:element name="RoomTransferIssuingDecisionsData_Documents" minOccurs="0">
						<xsd:element name="LegalDocumentsForTransfer" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Правоустанавливающие документы на переводимое жилое или нежилое помещение</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ReconstructionProject" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Проект переустройства и (или) перепланировки переводимого помещения</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>

					<!--Услуга: Предоставление документов (технического паспорта здания (строения) или выписки из него, поэтажного плана, плана земельного участка, экспликации к поэтажному плану, справки об инвентаризационной стоимости объекта недвижимости и иных документов)-->

					<xsd:element name="RealEstateObjectDocumentsData_Documents" minOccurs="0">
						<xsd:element name="LandPlotRight" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Копия документа, удостоверяющего или подтверждающего право на земельный участок</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="SelfGovernmentLegalActs" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Правовые акты органов местного самоуправления, разрешающие строительство объекта капитального строительства</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="CapitalConstructionObjectProjectDocumentation" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Проектная документация на объект капитального строительства</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<xsd:element name="CapitalConstructionTitleDocument" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Копия правоустанавливающиего документа об объекте капитального строитества</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="CapitalConstructionAddressAssigningCertificate" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Справка, подтверждающая присвоение адреса объекту капитального строительства</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>


					<xsd:element name="ApplicantProppertyData_Documents" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Сведения об имуществе заявителя</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="ApplicantProppertyOwershipCeertificate" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Справка о наличии прав на недвижимое имущество и сделок с ним, зарегистрированных до 01.01.1999 г. на каждого члена семьи (из БТИ)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ApplicantProppertyCostEstimation" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Документы, содержащие оценку стоимости имущества</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ApplicantProppertyTechnicalPassport" type="DocInfoType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Технический паспорт или иной документ, содержащий техническую информацию о жилом помещении</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>


					<xsd:element name="ApplicantIncomeData_Documents" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Сведения о доходах</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="ApplicantIncome_Document" type = "Generalize_DocumentType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Документ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>


					<xsd:element name="TransportData_Documents" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Сведения о доходах</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="TransportInfo_Document" type = "Generalize_DocumentType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Документ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>


					<xsd:element name="EducationalInstitutionChildrenEnrollmentData_Documents" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Документы для зачисления в образовательное учреждение</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="EducationalInstitutionChildrenEnrollment_Document" type = "Generalize_DocumentType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Документ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:element>


				</xsd:complexType>
















				<xsd:complexType name="DocInfoType">
					<xsd:annotation>
						<xsd:documentation>Сведения о вложении</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element name="CodeDocument" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Код документа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Name" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Имя файла документа</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="URL" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Относительный путь к файлу внутри архива</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="type" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>MIME-тип контента</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>


				<xsd:complexType name="FormResponseType">
					<xsd:annotation>
						<xsd:documentation>Ответ на подачу заявления или запроса на отмену заявления или текстового сообщения от заявителя в ведомство</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element name="changeOrderInfo" type="tns:changeOrderInfoType"/>
					</xsd:sequence>
				</xsd:complexType>
				<xsd:element name="FormResponse" type="tns:FormResponseType"/>
				<xsd:complexType name="changeOrderInfoType">
					<xsd:annotation>
						<xsd:documentation>Информация для изменения статуса заявления в ЛК ЕПГУ</xsd:documentation>
					</xsd:annotation>
					<xsd:sequence>
						<xsd:element name="orderId" type="tns:orderIdType"/>
						<xsd:element name="statusCode" type="tns:statusCodeType"/>
						<xsd:element name="comment" type="tns:string-2048" minOccurs="0"/>
						<xsd:element name="cancelAllowed" type="xsd:boolean" minOccurs="0"/>
						<xsd:element name="sendMessageAllowed" type="xsd:boolean" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
				<xsd:complexType name="orderIdType">
					<xsd:sequence>
						<xsd:annotation>
							<xsd:documentation>Номер заявки в ЛК ЕПГУ</xsd:documentation>
						</xsd:annotation>
						<xsd:element name="pguId" type="xsd:long"/>
					</xsd:sequence>
				</xsd:complexType>
				<xsd:complexType name="statusCodeType">
					<xsd:annotation>
						<xsd:documentation>Новый статус заявления</xsd:documentation>
					</xsd:annotation>
					<xsd:choice>
						<xsd:element name="orgCode" type="tns:string-20">
							<xsd:annotation>
								<xsd:documentation>Код ведомственного статуса</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="techCode" type="tns:string-20">
							<xsd:annotation>
								<xsd:documentation>Код статуса ЕПГУ</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:choice>
				</xsd:complexType>



			</xsd:schema>
